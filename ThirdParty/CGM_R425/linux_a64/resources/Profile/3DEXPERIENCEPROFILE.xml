  <Template syp:name="3DEXPERIENCEPROFILE" Target="CATPAfrProfile">
    <CATPAfrVariableList>
      <!--QUQ: Activate SW mouse behaviour-->
      <CATPAfrVariable syp:name="SWNavigationMode" vname="SWNavigationMode" default="false"/>
      <!--QUQ: Tessellation accuracy mode, 0 = proportional-->
      <CATPAfrVariable syp:name="3DAccuracy" vname="3DAccuracy" default="1"/>
      <!--R8Z: Tessellation accuracy, fixed value.-->
      <CATPAfrVariable syp:name="3DFxAccuracy" vname="3DFxAccuracy" default="0.05"/>
      <!--QUQ: Tessellation accuracy, proportional value.-->
      <CATPAfrVariable syp:name="3DPpAccuracy" vname="3DPpAccuracy" default="0.02"/>
      <!--QUQ: these cull values are important for edges in drawings, and manipulators-->
      <CATPAfrVariable syp:name="StaticCull" vname="StaticCull" default="2"/>
      <CATPAfrVariable syp:name="DynamicCull" vname="DynamicCull" default="3"/>
      <!--QUQ: 3 means 4x-->
      <CATPAfrVariable syp:name="FullSceneAntiAliasingMode" vname="FullSceneAntiAliasingMode" default="3"/>
      <!--QUQ: FSAA on even in dynamic drawing mode-->
      <CATPAfrVariable syp:name="AADynamic" vname="AADynamic" default="1"/>
      <!--GO3: Disable drawing of outlines in dynamic mode-->
      <CATPAfrVariable syp:name="OutlinesDynamic" vname="OutlinesDynamic" default="0"/>
      <!--QUQ: line aa on, but it should not actually make any difference given the other aa options-->
      <CATPAfrVariable syp:name="AntiAliasing" vname="AntiAliasing" default="1"/>
      <!--QUQ: this may or may not make a difference with MSAA on, depending on the driver-->
      <CATPAfrVariable syp:name="AntiAliasingOffset" vname="AntiAliasingOffset" default="0.2"/>
      <!--QUQ: default "custom view" style: outlines on, materials on, no smooth edges etc.-->
      <CATPAfrVariable syp:name="RenderingStyle.Default" vname="RenderingStyle.Default" default="4107"/>
      <!--QUQ: default "custom view" style: outlines on, materials on, no smooth edges etc.-->
      <!-- I'm not sure what the difference between RenderingStyle and CustomerViewMode .default is, so set both-->
      <CATPAfrVariable syp:name="CustomerViewMode.Default" vname="CustomerViewMode.Default" default="4107"/>
      <!--QUQ: this has been set for years on our side-->
      <CATPAfrVariable syp:name="AccuratePickingMode" vname="AccuratePickingMode" default="0"/>
      <!--VZ2: Default colors for bodies & surfaces to hide z-fighting in coplanar surfaces -->
      <CATPAfrVariable syp:name="VersionCombo" vname="VersionCombo" default="3"/>
      <!--FBJ: Default background color for "Basic" ambiance -->
      <CATPAfrVariable syp:name="BGMode" vname="BGMode" default="0"/>
      <CATPAfrVariable syp:name="Background" vname="Background" default="235 235 235"/>
      <!--Variables used for sketch customization in SWX profile-->
      <CATPAfrVariable syp:name="DYS_settings_GridSnapPt" vname="DYS_settings_GridSnapPt" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_BackgroundSmartPick" vname="DYS_settings_BackgroundSmartPick" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_ColorDefaultElts" vname="DYS_settings_ColorDefaultElts" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_ColorDiagWellDefinedElts" vname="DYS_settings_ColorDiagWellDefinedElts" default="65280" />
      <CATPAfrVariable syp:name="DYS_settings_ColorConstructElts" vname="DYS_settings_ColorConstructElts" default="9211020" />
      <CATPAfrVariable syp:name="DYS_settings_ColorProtectedElts" vname="DYS_settings_ColorProtectedElts" default="16776960" />
      <CATPAfrVariable syp:name="DYS_settings_DragWithEndPts" vname="DYS_settings_DragWithEndPts" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_ShowPosition" vname="DYS_settings_ShowPosition" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_MscNormalToMinTransfo" vname="DYS_settings_MscNormalToMinTransfo" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_SWXMakeGroupCreatingClosedGeometry" vname="DYS_settings_SWXMakeGroupCreatingClosedGeometry" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXMakeGroupClosingProfileWhenCreatingGeometry" vname="DYS_settings_SWXMakeGroupClosingProfileWhenCreatingGeometry" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXDragGroup" vname="DYS_settings_SWXDragGroup" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXReducedSnapsInferences" vname="DYS_settings_SWXReducedSnapsInferences" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWX2DCtxBarConstraint" vname="DYS_settings_SWX2DCtxBarConstraint" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStylePointMarker" vname="DYS_settings_SWXStylePointMarker" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStyleConstructionPointNormalView" vname="DYS_settings_SWXStyleConstructionPointNormalView" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStyleSketchGroupInShading" vname="DYS_settings_SWXStyleSketchGroupInShading" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStyleProduceSketchGroupSheetBodies" vname="DYS_settings_SWXStyleProduceSketchGroupSheetBodies" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStyleConstructionOutputFeature" vname="DYS_settings_SWXStyleConstructionOutputFeature" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_SWXStyleEnableHVPoints" vname="DYS_settings_SWXStyleEnableHVPoints" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_GridDisplay" vname="DYS_settings_GridDisplay" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_ShowPositionOnCursor" vname="DYS_settings_ShowPositionOnCursor" default="1" />
      <CATPAfrVariable syp:name="DYS_settings_RepeatCommandsAutomatically" vname="DYS_settings_RepeatCommandsAutomatically" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_UseAlternativeAppearanceNotInEdition" vname="DYS_settings_UseAlternativeAppearanceNotInEdition" default="0" />
      <CATPAfrVariable syp:name="DYS_settings_AlternativeColorNotInEdition" vname="DYS_settings_AlternativeColorNotInEdition" default="10342045" />
      <CATPAfrVariable syp:name="DYS_settings_AlternativeThicknessNotInEdition" vname="DYS_settings_AlternativeThicknessNotInEdition" default="1" />
      <!--Variables used in SWX profile update 19-09-2018-->
      <CATPAfrVariable syp:name="ArcSelection" vname="ArcSelection" default="1" />
      <CATPAfrVariable syp:name="AxisSystemLength" vname="AxisSystemLength" default="10" />
      <CATPAfrVariable syp:name="HighlightMode" vname="HighlightMode" default="3" />
