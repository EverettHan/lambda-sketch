      <VERTEX_STAGE>PostEffect_FullScreenQuadVS</VERTEX_STAGE>
      <FRAGMENT_STAGE>PostEffect_DeclarePS PostEffect_Helpers PostEffect_Texture_Helpers  PostEffect_Blit_RTAO</FRAGMENT_STAGE>
    </PASS>
  </TECHNIQUE>
  
</EFFECT>
