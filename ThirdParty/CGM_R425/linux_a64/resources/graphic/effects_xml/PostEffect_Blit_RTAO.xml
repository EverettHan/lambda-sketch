        </SHADER_OPTION>
        <SHADER_OPTION>
          <NAME>POSTEFFECT_NO_MAINCB</NAME>
          <OPTION_VALUE>1</OPTION_VALUE>
        </SHADER_OPTION>
      </SHADER_OPTIONS>
      
      <NAME>Pass0</NAME>
      <BLEND_STATE>NoBlend</BLEND_STATE>
      <DEPTH_STENCIL_STATE>DepthNoTestNoWrite</DEPTH_STENCIL_STATE>
      <RASTERIZER_STATE>Solid</RASTERIZER_STATE>
