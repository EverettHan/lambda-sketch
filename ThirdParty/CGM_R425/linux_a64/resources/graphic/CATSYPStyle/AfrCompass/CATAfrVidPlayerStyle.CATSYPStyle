          <CATVidCtlButtonIcon Name="BackwardButton"  syp:name="BackwardButton" syp:Template="BorderIcon" VisibleFlag="@PlayOn" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerBackward.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerBackward.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerBackward.png" DisabledTexture="Compass/PlayerV2/IP_CompassPlayerBackward.png" Click="@HandleEventBackward"   />
          <CATVidCtlButtonIcon Name="BackButton"  syp:name="BackButton" syp:Template="BorderIcon" VisibleFlag="@PlayOff" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerBack.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerBack.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerBack.png"  DisabledTexture="Compass/PlayerV2/IP_CompassPlayerBack.png"  Click="@HandleEventBack"  />
          <CATVidCtlButtonIcon Name="PlayReverseButton"   syp:name="PlayReverseButton" syp:Template="BorderIcon"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerPlayReverse.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerPlayReverse.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerPlayReverse.png" DisabledTexture="Compass/PlayerV2/IP_CompassPlayerPlayReverse.png" Click="@HandleEventPlayReverse" />
