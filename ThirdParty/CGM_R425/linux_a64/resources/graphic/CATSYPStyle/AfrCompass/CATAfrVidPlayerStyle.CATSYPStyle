            <CATVidSpacer RequestedMinimumDimension="2 2" CATVidLayGrid.IndexedConstraints="x=2 y=2 attach=RLTB"/>
          </CATVidLayGrid>

        </CATVidLayLayer>

        <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
          <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
          <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
            <CATVidCtlButtonIcon Name="LoopRunOnceButton" syp:name="LoopRunOnceButton" syp:Template="BorderIcon"
                                  LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                                 OffTexture="Compass/PlayerV2/I_CompassPlayerLoopRunOnce.png"
                                 OverTexture="Compass/PlayerV2/IF_CompassPlayerLoopRunOnce.png"
                                 OnTexture="Compass/PlayerV2/IP_CompassPlayerLoopRunOnce.png"
                                 IconDimension="22 22"  Click="@HandleEventLoop" VisibleFlag="1"/>
            <CATVidCtlButtonIcon Name="LoopRunContinuouslyButton" syp:name="LoopRunContinuouslyButton" syp:Template="BorderIcon"
                                  LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                                 OffTexture="Compass/PlayerV2/I_CompassPlayerLoopRunContinuously.png"
                                 OverTexture="Compass/PlayerV2/IF_CompassPlayerLoopRunContinuously.png"
                                 OnTexture="Compass/PlayerV2/IP_CompassPlayerLoopRunContinuously.png"
                                 IconDimension="22 22"  Click="@HandleEventLoop" VisibleFlag="0"/>
            <CATVidCtlButtonIcon Name="LoopRunContinuouslyBackAndForthButton" syp:name="LoopRunContinuouslyBackAndForthButton" syp:Template="BorderIcon"
                                  LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                                 OffTexture="Compass/PlayerV2/I_CompassPlayerLoopRunContinuouslyBackAndForth.png"
                                 OverTexture="Compass/PlayerV2/IF_CompassPlayerLoopRunContinuouslyBackAndForth.png"
                                 OnTexture="Compass/PlayerV2/IP_CompassPlayerLoopRunContinuouslyBackAndForth.png"
                                 IconDimension="22 22"  Click="@HandleEventLoop" VisibleFlag="0"/>
          </CATVidLayLayer>
          <CATVidSpacer RequestedMinimumDimension="5 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
          <CATVidCtlButtonIcon Name="OptionsButton" CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=RLTB"  syp:name="OptionsButton" syp:Template="BorderIcon"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerOptions.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerOptions.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerOptions.png"  DisabledTexture="Compass/PlayerV2/IP_CompassPlayerOptions.png"  Click="@HandleEventOptions" />

        </CATVidLayGrid>
      </CATVidLayGrid>

    </CATVidLayGrid>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunOnce">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>
      </CATSYPTrigger>

      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunContinuously">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

      </CATSYPTrigger>

      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunContinuouslyBackAndForth">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="1"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="1"/>
      </CATSYPTrigger>



    </CATSYPTriggerCollection>
  </Template>


