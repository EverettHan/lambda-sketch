          <CATVidCtlButtonIcon Name="PlayButton"  syp:name="PlayButton" syp:Template="BorderIcon" VisibleFlag="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerPlay.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerPlay.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerPlay.png"  DisabledTexture="Compass/PlayerV2/IP_CompassPlayerPlay.png"  Click="@HandleEventPlay" />
