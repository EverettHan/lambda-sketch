        <CATVidCtlButtonIcon Name="FirstButton"  syp:name="FirstButton" syp:Template="BorderIcon"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerFirst.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerFirst.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerFirst.png"  Click="@HandleEventFirst" />
