        </CATVidLayStack>
        <CATVidCtlButtonIcon Name="LastButton"  syp:name="LastButton" syp:Template="BorderIcon"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" IconDimension="22 22" OffTexture="Compass/PlayerV2/I_CompassPlayerLast.png" OverTexture="Compass/PlayerV2/IF_CompassPlayerLast.png" OnTexture="Compass/PlayerV2/IP_CompassPlayerLast.png"  DisabledTexture="Compass/PlayerV2/IP_CompassPlayerLast.png"  Click="@HandleEventLast" />


      </CATVidLayStack>
      <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB" RequestedMinimumDimension="0 15"/>
