<?xml version="1.0"?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">


  <!--*************************************************************************************************************************-->
  <!--                                           TOP BAR SIGNATURE                                                -->
  <!--*************************************************************************************************************************-->

  <!-- ======================================================================-->
  <!-- ===============   Old version for ODTs         =======================-->
  <!-- ======================================================================-->
  <Template syp:name="AfrPLMBarSignature" Target="CATAfrSYPContainerView">
    <CATAfrSYPContainerViewMetaData SypNamePosition="PLMBarPosition" AlwaysVisibleFlag="1" >
      <CATAfrSmartCtl  Viewer="@Viewer" Name="@Name" ViewParent="@Parent" InitialDockArea="TopDockArea" VisibleFlag="@WidgetsPanelVisibility" ToolbarFlag="1"
                  CreateDialogBoxFlag="1"
                  ConstraintsDialogBox="widthPercent=100 topPercent=0">           
          <CATAfrVidSmartContainer syp:name="SmartControlThreshold" Threshold1="630" Threshold2="800" Threshold3="1030" >
        <CATVidLayLayer>

          <!-- Background -->
          <CATVidGPSmartSquare GridWeight="row1=1" Texture="PLMBar/top-bg.png">
            <CATVidSpacer RequestedMinimumDimension="3 44" CATVidGPSmartSquare.GPConstraints="x=0 y=0 attach=RLTB"/>
            <CATVidSpacer RequestedMinimumDimension="3 32" CATVidGPSmartSquare.GPConstraints="x=0 y=1 attach=RLTB"/>
            </CATVidGPSmartSquare>

          <!-- / Background -->


          <CATVidLayGrid GridWeight="col1=1 row1=1">
            <!-- Compass (Fixed Area Left)-->
            <CATVidCtl syp:name="SupraContainerCompassPosition"  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>

            <!-- / Compass (Fixed Area Left)-->

               <!-- Appli Name + Search + User Name (Stretchable Area Center)-->
            <CATVidLayGrid GridWeight="col0=1 col1=1 col2=1" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">

              <!-- Appli name (Left)-->

                <CATVidLayGrid  syp:name="Left"  LayoutWhenUnvisibleFlag="0" GridWeight ="col1=1 row2=1"  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">

                  <CATVidSpacer RequestedMinimumDimension="18 9" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                  <CATVidCtl syp:name="CurrentAppliNamePosition" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>
                  <CATVidSpacer  CATVidLayGrid.IndexedConstraints="x=0 y=2 xSpan=2 attach=RLTB"/>
                </CATVidLayGrid>


              <!-- / Appli name -->


              <!-- Search + Refinement (Center)-->
              <CATVidLayLayer syp:name="Center"  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLT">
                <!-- Blocking Square For record Search -->
                <CATVidGPSquare syp:name="BlockingSquareForSearch" Name="BlockingSquareForSearch" UseManipulatorFlag="1" VisibleFlag="0"/>
                <CATVidLayGrid GridWeight="col2=2">
                  <CATVidSpacer RequestedMinimumDimension="0 4" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                  <!--<CATVidSpacer RequestedMinimumDimension="@SpacerDimension ElementName=SmartContainer" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>-->
                  <CATAfrVidSmartContainer syp:name="SmartContainer" CenteredFlag="1"  MaximumWidth="328" CATVidLayGrid.IndexedConstraints="x=2 y=1 attach=RLTB">
                    <CATVidLayGrid GridWeight="col0=1 row0=1" >
                      <!-- Search-->
                      <CATVidCtl  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RL" >
                        <CATVidLayLayer RequestedMinimumDimension="100 24" LayoutWhenUnvisibleFlag="0">
                          <CATVidGPSmartSquare  GridWeight="col1=1 row1=1" Texture="PLMBar/Search_backround.png">
                            <CATVidSpacer RequestedMinimumDimension="8 8" CATVidGPSmartSquare.GPConstraints="y=0 x=0 attach=RLTB"/>
                            <CATVidSpacer RequestedMinimumDimension="4 8" CATVidGPSmartSquare.GPConstraints="y=1 x=1 attach=RLTB"/>
                            <CATVidSpacer RequestedMinimumDimension="8 8" CATVidGPSmartSquare.GPConstraints="y=2 x=2 attach=RLTB"/>
                          </CATVidGPSmartSquare>
                          <CATVidLayGrid  GridWeight="col1=1 row2=1" >
                            <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                            <CATVidLayGrid GridWeight="col1=1"  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
                              <!---->
                              <CATVidCtl syp:name="SearchMenuPosition" VisibleFlag="0"  LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                              <CATVidLayLayer syp:name="SearchFieldPosition" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RL"/>

                            </CATVidLayGrid>
                            <CATVidSpacer RequestedMinimumDimension="8 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                          </CATVidLayGrid>
                        </CATVidLayLayer>
                      </CATVidCtl>
                      <CATVidSpacer RequestedMinimumDimension="1 6" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
                      <!-- / Search-->
                      <!-- Refinement Button-->
                      <CATVidCtl syp:name="SearchRefinement" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
                      <!-- / Refinement Button-->
                    </CATVidLayGrid>

                  </CATAfrVidSmartContainer>

                  <!--<CATVidSpacer RequestedMinimumDimension="@SpacerDimension ElementName=SmartContainer"  CATVidLayGrid.IndexedConstraints="x=3 y=1 attach=RLTB"/>-->

                </CATVidLayGrid>
              </CATVidLayLayer>


            <!-- / Search + Refinement (Center)-->

              <!-- User name (Right)-->
              <CATVidLayGrid syp:name="Right"  LayoutWhenUnvisibleFlag="0" GridWeight="col0=2 col1=1 row2=1"   CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB">


                <CATVidGPSquare RequestedMinimumDimension="1 9" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                <CATVidCtl syp:name="UserNamePosition" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>
                  <CATVidSpacer  CATVidLayGrid.IndexedConstraints="x=0 y=2 xSpan=2 attach=RLTB"/>
                </CATVidLayGrid>


              <!-- / User name -->
            </CATVidLayGrid>
          
             

            <!-- / Appli Name + Search + User Name (Stretchable Area Center)-->

            <!-- Icons (Fixed Area Right)-->
            <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=RLT">
              <!-- Blocking Square  for menus-->
              <!-- Blocking Square For record Search -->
              <CATVidGPSquare syp:name="BlockingSquareForMenu" Name="BlockingSquareForMenu" UseManipulatorFlag="1" VisibleFlag="0"/>
              <CATVidLayGrid  GridWeight="col0=1 row2=1" >
                <CATVidSpacer RequestedMinimumDimension="1 4" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                <CATVidLayStack syp:name="Container" CellBorder="0" Items="@ToolsItems Mode=OneWayToSource" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RTB"/>
                <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
              </CATVidLayGrid>
            </CATVidLayLayer>

            <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
          </CATVidLayGrid>
          <!--<CATVidLayGrid GridWeight="col1=1 row1=1" Color="200 100 100 100">
            <CATVidCtl syp:name="SupraContainerCompassPosition"   CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
            <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>
          </CATVidLayGrid>-->
          <CATVidGPSquare syp:name="BlockingSquareForRecord" Name="BlockingSquareForRecordTopBar" UseManipulatorFlag="1" VisibleFlag="0"/>
        </CATVidLayLayer>

        </CATAfrVidSmartContainer>
      </CATAfrSmartCtl>
    </CATAfrSYPContainerViewMetaData>

    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Threshold1Flag" Value="0">
         <CATSYPSetter Property="VisibleFlag" TargetName="CurrentAppliNamePosition" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold1Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="CurrentAppliNamePosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="0"/>
        <!--<CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>-->
        <!--<CATSYPSetter Property="MaximumWidth" TargetName="SmartContainer" Value="200"/>-->
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold2Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="CurrentAppliNamePosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold3Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="CurrentAppliNamePosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="1"/>
 
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>
  <!-- ======================================================================-->
  <!-- ===============            Top Bar Version 2      =======================-->
  <!-- ======================================================================-->
  <Template syp:name="AfrPLMBarSignature" Target="CATAfrSYPContainerView" syp:version="2">
    <CATAfrSYPContainerViewMetaData SypNamePosition="PLMBarPosition" AlwaysVisibleFlag="1" >
      <CATAfrSmartCtl  Viewer="@Viewer" Name="@Name" ViewParent="@Parent" InitialDockArea="TopDockArea" VisibleFlag="@WidgetsPanelVisibility" ToolbarFlag="1"
                  CreateDialogBoxFlag="1"
                  ConstraintsDialogBox="widthPercent=100 topPercent=0">
        <CATAfrVidSmartContainer syp:name="SmartControlThreshold" Threshold1="540" Threshold2="650" Threshold3="1030" >
          <!-- version if you modify this line -->
          <CATVidLayGrid GridWeight="row1=1">

            <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">

              <!-- Background -->
              <CATVidGPSmartSquare GridWeight="row1=1" Texture="PLMBar/top-bg.png">
                <CATVidSpacer RequestedMinimumDimension="3 44" CATVidGPSmartSquare.GPConstraints="x=0 y=0 attach=RLTB"/>
                <CATVidSpacer RequestedMinimumDimension="3 32" CATVidGPSmartSquare.GPConstraints="x=0 y=1 attach=RLTB"/>
              </CATVidGPSmartSquare>

              <!-- / Background -->


              <!--Layer For Stretchable Content-->
              <CATVidLayGrid GridWeight="col1=1">
                <!--Spacer For Compass : To Hide When Compass is inserted in its own view (Le Borgne)-->
                <CATVidSpacer syp:name="SpacerCompass" RequestedMinimumDimension="@Dimension ElementName=SupraContainerCompassPosition" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                <!--Strecchable Grid-->
                <CATVidLayGrid GridWeight="col0=3.875 col1=2.25 col2=3.875 row2=1" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">

                  <!--Top Margin -->
                  <CATVidSpacer RequestedMinimumDimension="0 6" CATVidLayGrid.IndexedConstraints="x=0 y=0 xSpan=3 attach=RLTB"/>
                  <!--Bottom Margin -->
                  <CATVidSpacer  CATVidLayGrid.IndexedConstraints="x=0 y=2 xSpan=3 attach=RLTB"/>
                  <!--Top Bar Title (Left)-->
                  <CATVidLayGrid  syp:name="Left"  LayoutWhenUnvisibleFlag="0" GridWeight ="col1=1 row2=1"  CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB">

                    <CATVidSpacer RequestedMinimumDimension="0 5" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                    <CATVidLayGrid syp:name="TopBarTitle" GridWeight="col3=1" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB">
                      <CATVidGPLabel LayoutWhenUnvisibleFlag="0" Text="3D" BoldFlag="1" FontName="3ds" TextHeight ="19" VerticalAlignment="1" HorizontalAlignment="0"  CropPolicy="NoCrop" TextColor="255 255 255 255" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                      <CATVidGPLabel LayoutWhenUnvisibleFlag="0" Text="EXPERIENCE " BoldFlag="0" FontName="3ds" TextHeight ="19" VerticalAlignment="1" HorizontalAlignment="0"  CropPolicy="NoCrop" TextColor="255 255 255 255" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
                      <CATVidCtl syp:name="CurrentBrandNamePosition"  CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                      <CATVidCtl syp:name="CurrentAppliNamePosition"  CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=RLTB"/>
                    </CATVidLayGrid>

                    <CATVidSpacer  CATVidLayGrid.IndexedConstraints="x=0 y=2 xSpan=2 attach=RLTB"/>
                  </CATVidLayGrid>
                  <!--/ Top Bar Title-->

                  <!--Search (Center)-->
                  <CATVidLayGrid GridWeight="row1=1" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB">
                    <CATVidLayLayer RequestedMinimumDimension="100 24" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
                      <CATVidGPSmartSquare  GridWeight="col1=1 row1=1" Texture="PLMBar/Search_backround.png">
                        <CATVidSpacer RequestedMinimumDimension="8 8" CATVidGPSmartSquare.GPConstraints="y=0 x=0 attach=RLTB"/>
                        <CATVidSpacer RequestedMinimumDimension="4 8" CATVidGPSmartSquare.GPConstraints="y=1 x=1 attach=RLTB"/>
                        <CATVidSpacer RequestedMinimumDimension="8 8" CATVidGPSmartSquare.GPConstraints="y=2 x=2 attach=RLTB"/>
                      </CATVidGPSmartSquare>
                      <CATVidLayGrid  GridWeight="col1=1 row2=1" >
                        <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                        <CATVidLayGrid GridWeight="col1=1"  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
                          <!---->
                          <CATVidCtl syp:name="SearchMenuPosition" VisibleFlag="0"  LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                          <CATVidLayLayer syp:name="SearchFieldPosition" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RL"/>

                        </CATVidLayGrid>
                        <CATVidSpacer RequestedMinimumDimension="8 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                      </CATVidLayGrid>
                    </CATVidLayLayer>
                    <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
                  </CATVidLayGrid>

                  <!--/ Search-->

                  <!--Tag + User Name + Spacer (Right)-->
                  <CATVidLayGrid GridWeight="col1=1" CATVidLayGrid.IndexedConstraints="x=2 y=1 attach=RLTB">

                    <!--Tag Button -->
                    <CATVidCtl syp:name="SearchRefinement" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                    <!--User Name-->
                    <CATVidLayGrid  LayoutWhenUnvisibleFlag="0" GridWeight="col0=2 col1=1 row2=1"   CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
                      <CATVidGPSquare RequestedMinimumDimension="1 5" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                      <CATVidCtl syp:name="UserNamePosition" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>
                      <CATVidSpacer  CATVidLayGrid.IndexedConstraints="x=0 y=2 xSpan=2 attach=RLTB"/>
                    </CATVidLayGrid>
                    <!--Remaining Margin to fit with Top Bar Menus (200 -144 = 56) + 4 for margin between user name-->
                    <CATVidSpacer RequestedMinimumDimension="70 1" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                  </CATVidLayGrid>

                  <!--/ Tag + User Name + Spacer-->
                </CATVidLayGrid>
                <!--Spacer with Same dimension of compass to always have a centered Grid-->
                <CATVidSpacer  RequestedMinimumDimension="144 1" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
              </CATVidLayGrid>
              <!--/ Layer For Stretchable Content-->


              <!--Layer For Fixed Content-->
              <CATVidLayGrid GridWeight="col1=1">
                <!--Container for Compass -->
                <CATVidCtl syp:name="SupraContainerCompassPosition"  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                <!--Dummy Spacer-->
                <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
                <!--Container for Top Bar Menu (Should be have a width of 200 according to the spec :32+8+32+8+32+8+32+8+32+8)-->
                <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLT">
                  <!-- Blocking Square  for menus-->
                  <!-- Blocking Square For record Search -->
                  <CATVidGPSquare syp:name="BlockingSquareForMenu" Name="BlockingSquareForMenu" UseManipulatorFlag="1" VisibleFlag="0"/>
                  <CATVidLayGrid  GridWeight="col0=1 row2=1" >
                    <CATVidSpacer RequestedMinimumDimension="1 4" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                    <CATVidLayStack syp:name="Container" CellBorder="0" Items="@ToolsItems Mode=OneWayToSource" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RTB"/>
                    <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                  </CATVidLayGrid>
                </CATVidLayLayer>
                <!--/Container for Top Bar Menu-->
              </CATVidLayGrid>
              <!--/Layer For Fixed Content-->

              <CATVidGPSquare syp:name="BlockingSquareForRecord" Name="BlockingSquareForRecordTopBar" UseManipulatorFlag="1" VisibleFlag="0"/>
            </CATVidLayLayer>
            <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
          </CATVidLayGrid>


        </CATAfrVidSmartContainer>
      </CATAfrSmartCtl>
    </CATAfrSYPContainerViewMetaData>

    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Threshold1Flag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="TopBarTitle" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold1Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="TopBarTitle" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="0"/>
        <!--<CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>-->
        <!--<CATSYPSetter Property="MaximumWidth" TargetName="SmartContainer" Value="200"/>-->
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold2Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="TopBarTitle" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="Threshold3Flag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="TopBarTitle" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="SearchMenuPosition" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="UserNamePosition" Value="1"/>

      </CATSYPTrigger>
      <CATSYPTrigger Property="MobileFlag" Value="1">
        <CATSYPSetter Property="Threshold1" TargetName="SmartControlThreshold" Value="500"/>


      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>
  <!-- ======================================================================-->
  <!-- ===============            Top Bar Version 3      =======================-->
  <!-- ======================================================================-->
  <!-- ======================================================================-->
  <!-- ===============            MAIN CONTAINER       =======================-->
  <!-- ======================================================================-->
  <Template syp:name="AfrPLMBarSignature" Target="CATAfrSYPContainerView" syp:version="3">
    <CATAfrSYPContainerViewMetaData SypNamePosition="TopBarContainer" AlwaysVisibleFlag="1" >
      <CATAfrSmartCtl  Viewer="@Viewer" Name="@Name" ViewParent="@Parent" InitialDockArea="TopDockArea" VisibleFlag="@WidgetsPanelVisibility" ToolbarFlag="1"
                  CreateDialogBoxFlag="@CreateDialogBoxFlag"
                  ConstraintsDialogBox="widthPercent=100 topPercent=0">
        <CATAfrVidSmartContainer syp:name="SmartControlThreshold" Threshold1="540" Threshold2="650" Threshold3="1030" >
          <!-- version if you modify this line -->
          <CATVidLayGrid GridWeight="row1=1">

            <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">

              <!-- Background -->
              <CATVidGPSmartSquare  GridWeight="row1=1" Texture="PLMBar/top-bg.png">
                <CATVidSpacer syp:name="BackgroundTopBar" RequestedMinimumDimension="3 44" CATVidGPSmartSquare.GPConstraints="x=0 y=0 attach=RLTB"/>
                <CATVidSpacer RequestedMinimumDimension="3 32" CATVidGPSmartSquare.GPConstraints="x=0 y=1 attach=RLTB"/>
              </CATVidGPSmartSquare>

              <!-- / Background -->


              <!-- Main Content-->
              <CATVidLayGrid  GridWeight="row1=1" >
                <CATVidLayLayer  RequestedMinimumDimension="@RequestedMinimumDimension ElementName=BackgroundTopBar" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">



                  <!--Layer For Stretchable Content-->
                  <CATVidLayGrid GridWeight="col1=1">
                    <!--Spacer For Compass : To Hide When Compass is inserted in its own view (Le Borgne)-->
                    <CATVidSpacer syp:name="SpacerCompass" RequestedMinimumDimension="@Dimension ElementName=SupraContainerCompassPosition" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>

                    <!--Stretchable Side Attach-->
                    <CATVidLaySideAttach  IgnoreChildrenMinimumDimensionFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"  RequestedMinimumDimension="232 1">
                      <!--Top Bar Title (Left)-->
                      <CATVidLayGrid syp:name="Left"  LayoutWhenUnvisibleFlag="0" RequestedMinimumDimension="@RequestedMinimumDimension ElementName=BackgroundTopBar"  CATVidLaySideAttach.IndexedConstraints="leftPercent=0 widthPercent=38.75">


                        <CATVidLayGrid syp:name="TopBarTitle" GridWeight="col3=1" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RL">
                          <CATVidGPLabel LayoutWhenUnvisibleFlag="0" Text="3D" BoldFlag="1" FontName="3ds" TextHeight ="19" VerticalAlignment="1" HorizontalAlignment="0"  CropPolicy="NoCrop" TextColor="255 255 255 255" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                          <CATVidGPLabel LayoutWhenUnvisibleFlag="0" Text="EXPERIENCE " BoldFlag="0" FontName="3ds" TextHeight ="19" VerticalAlignment="1" HorizontalAlignment="0"  CropPolicy="NoCrop" TextColor="255 255 255 255" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
                          <CATVidCtl syp:name="CurrentBrandNamePosition" LayoutWhenUnvisibleFlag="0" VisibleFlag="0"  CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
                          <CATVidCtl syp:name="CurrentAppliNamePosition"  CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=RLTB"/>
                        </CATVidLayGrid>


                      </CATVidLayGrid>
                      <!--/ Top Bar Title-->

                      <!--Search (Center)-->
                      <CATVidLayGrid GridWeight="row2=1 col0=1" RequestedMinimumDimension="@RequestedMinimumDimension ElementName=BackgroundTopBar" CATVidLaySideAttach.IndexedConstraints="leftPercent=50 widthPercent=22.25">
                        <CATVidLayLayer RequestedMinimumDimension="100 24" LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RL">


                          <CATVidGPNinePatchSquare  ScalableAreaBorder="6" Texture="PLMBar/Search_backround.png"></CATVidGPNinePatchSquare>
                          <CATVidLayGrid  GridWeight="col1=1 row2=1" >
                            <CATVidSpacer RequestedMinimumDimension="4 0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                            <CATVidLayGrid GridWeight="col1=1"  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
                              <!---->
                              <CATVidCtl syp:name="SearchMenuPosition" VisibleFlag="1"  LayoutWhenUnvisibleFlag="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                              <CATVidLayLayer syp:name="SearchFieldPosition" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>

                            </CATVidLayGrid>
                            <CATVidSpacer RequestedMinimumDimension="8 0" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>

                          </CATVidLayGrid>
                        </CATVidLayLayer>
                        <!--Tag Button -->
