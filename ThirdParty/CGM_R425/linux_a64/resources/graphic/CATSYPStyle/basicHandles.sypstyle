  <!--=================================================================-->
  <!-- CATV3DCtlHandleTranslation : Arrow Template                     -->
  <!--=================================================================-->
  <Template syp:name="Arrow" Target="CATV3DCtlHandleTranslation">
    <CATV3DLay3D syp:name="Lay3D">
      <CATV3DGPEmbeddedVid syp:name="GPManip" UseManipulatorFlag="1" Adjustment="ToMinValue Centered Centered" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick">
        <CATVidLayGrid>
          <CATVidGPSquare syp:name="OffGP" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="TranslationArrow.png" RequestedMinimumDimension="134 36" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" />
          <CATVidGPSquare syp:name="OverGP" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="TranslationArrow_Over.png" RequestedMinimumDimension="134 36" VisibleFlag="1" TextureAlpha="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
          <CATVidGPSquare syp:name="OnGP" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="TranslationArrow_On.png" RequestedMinimumDimension="134 36" VisibleFlag="1" TextureAlpha="0" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
          <CATVidGPSquare syp:name="Space" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB" RequestedMinimumDimension="16 36"/>
        </CATVidLayGrid>
      </CATV3DGPEmbeddedVid>
      <CATV3DCtlHandleLabel syp:name="GPLabel" syp:Template="Default" CATV3DLay3D.Constraints="orientation=ParallelToScreen pxFlag=0" Adjustment="-60 -30 0" Text="@LabelText" VisibleFlag="@LabelVisibleFlag" BoldFlag="1" CropPolicy="NoCrop" TextColor="0.5 225" TextHeight="30" EnableFlag="@EnableFlag" Translate="@HandleDrag" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick"/>
    </CATV3DLay3D>
