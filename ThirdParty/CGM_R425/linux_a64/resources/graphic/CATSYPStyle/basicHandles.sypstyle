            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.2"  To="75"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.2"  To="75"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.2"  To="0"/>
