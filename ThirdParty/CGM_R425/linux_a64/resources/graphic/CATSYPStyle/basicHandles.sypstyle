            <CATVidGPSquare syp:name="Border" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Translation_Arrow_Border.png" ColorizeBlackPixelTo="0 0 0 255" TextureAlpha="127" PickFlag="0" />
          </CATVidLayLayer>
          <!-- This square is used to ensure that the computation of BB is still ok. after refactoring 12/13-->
          <CATVidGPSquare RequestedMinimumDimension="16 60" CATVidLayGrid.IndexedConstraints="x=1 y=0" PickFlag="0"/>
        </CATVidLayGrid>
      </CATV3DGPEmbeddedVid>
      <CATV3DCtlHandleLabel syp:name="GPLabel" syp:Template="Default" CATV3DLay3D.Constraints="orientation=ParallelToScreen pxFlag=0" Adjustment="-110 -50 0" Text="@LabelText" VisibleFlag="@LabelVisibleFlag" BoldFlag="1" CropPolicy="NoCrop" TextColor="0.5 225" TextHeight="55" EnableFlag="@EnableFlag" Translate="@HandleDrag" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick"/>
    </CATV3DLay3D>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="CheckState" Value="1">
        <CATSYPSetter Property="TextureAlpha" TargetName="CheckFill" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="CheckState" Value="0">
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"   Duration="0:0:0.2" To="0"/>
