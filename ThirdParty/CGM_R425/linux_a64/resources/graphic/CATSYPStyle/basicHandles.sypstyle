        <CATVidGPSquare syp:name="Fill"       TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Fill.png" ColorizeBlackPixelTo="255 255 255 255" TextureAlpha="255" PickFlag="1"/>
        <CATVidGPSquare syp:name="CheckFill"  TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Fill.png" ColorizeBlackPixelTo="245 130 32 255"  TextureAlpha="0" PickFlag="1"/>
        <CATVidGPSquare syp:name="OverShadow" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_OverShadow.png" PickFlag="1"/>
        <CATVidGPSquare syp:name="Border"     TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Border.png" ColorizeBlackPixelTo="0 0 0 255" TextureAlpha="115" PickFlag="1"/>
      </CATVidLayLayer>
    </CATV3DGPEmbeddedVid>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="CheckState" Value="1">
        <CATSYPSetter Property="TextureAlpha" TargetName="CheckFill" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="CheckState" Value="0">
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"   Duration="0:0:0.2" To="0"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="DefaultManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Shadow" Value="1"/>
        <CATSYPSetter Property="PickFlag" TargetName="Fill" Value="1"/>
        <CATSYPSetter Property="PickFlag" TargetName="CheckFill" Value="1"/>
        <CATSYPSetter Property="PickFlag" TargetName="OverShadow" Value="1"/>
        <CATSYPSetter Property="PickFlag" TargetName="Border" Value="1"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.5" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.5"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.5"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Border"  Duration="0:0:0.5"  To="115"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="DisableManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Shadow" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="Fill" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="CheckFill" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="OverShadow" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="Border" Value="0"/>
        <CATSYPBeginStoryboard>
