<?xml version="1.0"?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">
  <!--============================== CATV3DCtlHandle Templates ==============================-->

  <!--=================================================================-->
  <!-- CATV3DCtlHandle : ModificationHandle Template                   -->
  <!--=================================================================-->
  <Template syp:name="ModificationHandle" Target="CATV3DCtlHandle">
    <CATV3DGPEmbeddedVid syp:name="GPManip" UseManipulatorFlag="1" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick">
      <CATVidLayLayer>
        <CATVidGPSquare RequestedMinimumDimension="32 32" Name="PickZone" PickFlag="1"/>
        <CATVidLayGrid>
          <CATVidLayLayer syp:name="GenericLayer" CATVidLayGrid.IndexedConstraints="x=0 y=0">
            <CATVidLayLayer>
              <CATVidGPSquare RequestedMinimumDimension="32 32" ColorizeBlackPixelTo="255 255 255 165" syp:name="OffLayer" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="CATLiveShapeMouseModificationHandle_Off.png"/>
            </CATVidLayLayer>
            <CATVidGPSquare RequestedMinimumDimension="32 32" syp:name="OverLayer" ColorizeBlackPixelTo="80 160 200 255" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="CATLiveShapeMouseModificationHandle_Over.png" />
            <CATVidGPSquare RequestedMinimumDimension="32 32" syp:name="OutLineLayer" ColorizeBlackPixelTo="255 255 255 165" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="CATLiveShapeMouseModificationHandle_Outline.png" />
            <CATVidGPSquare RequestedMinimumDimension="32 32" syp:name="OriginLayer" VisibleFlag="0" ColorizeBlackPixelTo="255 255 255 165" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="CATLiveShapeMouseModificationHandle_Origin.png" />
            <CATVidGPSquare RequestedMinimumDimension="32 32" syp:name="OnLayer" ColorizeBlackPixelTo="247 148 30 255" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="CATLiveShapeMouseModificationHandle_On.png" />
          </CATVidLayLayer>
        </CATVidLayGrid>
      </CATVidLayLayer>
    </CATV3DGPEmbeddedVid>

    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="CheckState" Value="1">
        <CATSYPSetter Property="Texture" TargetName="OffLayer"  Value="CATLiveShapeMouseModificationHandle_On.png"/>
        <CATSYPSetter Property="Texture" TargetName="OnLayer"  Value="CATLiveShapeMouseModificationHandle_Off.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="CheckState" Value="0">
        <CATSYPSetter Property="Texture" TargetName="OnLayer"  Value="CATLiveShapeMouseModificationHandle_On.png"/>
        <CATSYPSetter Property="Texture" TargetName="OffLayer"  Value="CATLiveShapeMouseModificationHandle_Off.png"/>
      </CATSYPTrigger>

      <CATSYPTrigger Property="OverFlag" Value="1">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OffLayer" Value="80 160 200 255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="OverLayer" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="OutLineLayer" Value="0"/>
      </CATSYPTrigger>

      <CATSYPTrigger Property="OverFlag" Value="0">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OffLayer" Value="255 255 255 165"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverLayer" Duration="0:0:0.2" To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OutLineLayer" Duration="0:0:0.2" To="255"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="OnFlag" Value="1">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OffLayer" Value="247 148 30 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OverLayer" Value="247 148 30 255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OnLayer" Duration="0:0:0.05" To="255"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="OnFlag" Value="0">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OffLayer" Value="255 255 255 165"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="OverLayer" Value="80 160 200 255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OnLayer" Duration="0:0:0.2" To="0"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>

  <!--=================================================================-->
  <!-- CATV3DCtlHandle : TranslationPoint Template                     -->
  <!--                   Used in CATV3DCtlRobot                        -->
  <!--=================================================================-->
  <Template syp:name="TranslationPoint" Target="CATV3DCtlHandle" syp:version="3">
    <CATV3DLay3D syp:name="Lay3D">
      <CATV3DGPEmbeddedVid syp:name="GPManip" UseManipulatorFlag="1" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" DoubleClick="@HandleDoubleClick" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick">
        <CATVidLayLayer syp:name="MainLayer" RequestedMinimumDimension="70 70">
          <CATVidGPSquare syp:name="Polygon" PickFlag="1"/>
          <CATVidGPSquare syp:name="Shadow"     TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Shadow.png"      PickFlag="0"/>
          <CATVidGPSquare syp:name="Fill"       TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Fill.png"        ColorizeBlackPixelTo="255 255 255 255" TextureAlpha="255" PickFlag="0"/>
          <CATVidGPSquare syp:name="CheckFill"  TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Fill.png"        ColorizeBlackPixelTo="245 130 32 255"  TextureAlpha="0" PickFlag="0"/>
          <CATVidGPSquare syp:name="OverShadow" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_OverShadow.png"  PickFlag="0"/>
          <CATVidGPSquare syp:name="Border"     TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Handle_Point_Border.png"      ColorizeBlackPixelTo="0 0 0 255" TextureAlpha="115" PickFlag="0"/>
        </CATVidLayLayer>
      </CATV3DGPEmbeddedVid>
      <CATV3DCtlHandleLabel syp:name="GPLabel" syp:Template="Default" CATV3DLay3D.Constraints="orientation=ParallelToScreen pxFlag=0" Adjustment="0 -55 0" Text="@LabelText" VisibleFlag="@LabelVisibleFlag" BoldFlag="1" CropPolicy="NoCrop" TextColor="0.5 225" TextHeight="55" EnableFlag="@EnableFlag" Translate="@HandleDrag" Entry="@HandleEntry" Leave="@HandleLeave" Press="@HandlePress" EndDrag="@HandleEndDrag" BeginDrag="@HandleBeginDrag" Drag="@HandleDrag" Release="@HandleRelease" EndRightClick="@HandleEndRightClick" MoveOver="@HandleMoveOver" RightClick="@HandleRightClick"/>
    </CATV3DLay3D>
    
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="CheckState" Value="1">
        <CATSYPSetter Property="TextureAlpha" TargetName="CheckFill" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="CheckState" Value="0">
        <CATSYPSetter Property="TextureAlpha" TargetName="Fill" Value="255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"   Duration="0:0:0.2" To="0"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="DefaultManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Polygon" Value="1"/>
        <CATSYPSetter Property="PickFlag" TargetName="GPLabel" Value="1"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.5" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.5"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.5"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Border"  Duration="0:0:0.5"  To="115"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="DisableManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Polygon" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="GPLabel" Value="0"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.2"  To="75"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.2"  To="75"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Border"  Duration="0:0:0.2"  To="100"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="GhostManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Polygon" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="GPLabel" Value="0"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.2"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.2"  To="255"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Border"  Duration="0:0:0.2"  To="115"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="ManipulationState" Value="InvisibleManipulation">
        <CATSYPSetter Property="PickFlag" TargetName="Polygon" Value="0"/>
        <CATSYPSetter Property="PickFlag" TargetName="GPLabel" Value="0"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.5" TransitionTimelineFlag="1">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Shadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Fill"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="CheckFill"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="OverShadow"  Duration="0:0:0.5"  To="0"/>
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Border"  Duration="0:0:0.5"  To="0"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
        
      <CATSYPTrigger Property="OverFlag" Value="1">
        <CATSYPSetter Property="TextColor" TargetName="GPLabel" Value="1.0 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Fill" Value="147 215 238 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="CheckFill" Value="147 215 238 255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.1" TransitionTimelineFlag="1">
            <CATSYPPoint2DfAnimation CATSYPStoryboard.TargetProperty="RequestedMinimumDimension" CATSYPStoryboard.TargetName="MainLayer"   Duration="0:0:0.1" To="84 84"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
      <CATSYPTrigger Property="OverFlag" Value="0">
        <CATSYPSetter Property="TextColor" TargetName="GPLabel" Value="0.5 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Fill"    Value="255 255 255 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="CheckFill" Value="245 130 32 255"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.1" TransitionTimelineFlag="1">
            <CATSYPPoint2DfAnimation CATSYPStoryboard.TargetProperty="RequestedMinimumDimension" CATSYPStoryboard.TargetName="MainLayer"   Duration="0:0:0.1" To="70 70"/>
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>

      <CATSYPTrigger Property="PressedFlag" Value="1">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Fill" Value="27 110 156 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Border" Value="255 255 255 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="CheckFill" Value="27 110 156 255"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="PressedFlag" Value="0">
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Fill" Value="255 255 255 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="Border" Value="0 0 0 255"/>
        <CATSYPSetter Property="ColorizeBlackPixelTo" TargetName="CheckFill" Value="245 130 32 255"/>
      </CATSYPTrigger>

    </CATSYPTriggerCollection>
  </Template>

