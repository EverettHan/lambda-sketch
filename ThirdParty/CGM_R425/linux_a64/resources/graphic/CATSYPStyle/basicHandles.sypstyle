            <!-- In this case we could use a square, but the polygon keeps it dimension when the layout grow. We assume it's the design.-->
            <CATVidGPPolygon syp:name="Polygon" Contour="30 60 280 60 280 0 30 0" PickFlag="1"/>
            <CATVidGPSquare syp:name="Shadow" TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Translation_Arrow_Shadow.png" ColorizeBlackPixelTo="0 0 0 255" PickFlag="0"/>
            <CATVidGPSquare syp:name="Fill"   TextureFilter="1" MinificationFilter="LinearMipMapLinear" Texture="Translation_Arrow_Fill.png"   ColorizeBlackPixelTo="255 255 255 255" TextureAlpha="255" PickFlag="0" />
