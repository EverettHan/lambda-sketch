  <!--*************************************************************************************************************************-->
  <!--                                           AfrTopBarHomeMenu container                                                   -->
  <!--*************************************************************************************************************************-->
  <DataTemplate syp:name="CATCAfrMenu_AfrTopBarHomeMenu" >
    <!--A Syp Name to allow positionning the home menu in the Top Bar (see below)-->
    <!--SypNamePosition="HomeMenuPosition"-->
    <CATAfrSYPContainerViewMetaData syp:name="MetaData" NameBis="@Name"  IconBoxFlag="1" NormalIcon="@Icon" >
      <CATAfrVidTopBarMenu syp:name="Container" syp:Template="HomeMenu"
                           RecordName="@Name"
                           Title="@Title"
                           ContentVisibleFlag="@ContentVisibleFlag Mode=TwoWays"
                           VisibleFlag="@UsefulFlag" LayoutWhenUnvisibleFlag="0"
                           ContentShown="@HandleEventMenuDeployed"
                           PresenterClick="@HandleActionEvent"
                           PresenterTooltip="@Tooltip">

        <!--<CATVidGPSquare RequestedMinimumDimension="44 44" Color="0 239 239 230" />-->
        <CATAfrVidTopBarMenu.PresenterCtl>
          <CATAfrVidExtraViewPresenter syp:name="ExtraViewPresenter" ExtraViewDataTemplate="AfrTopBarHomeMenu_Title"
                                      PropertiesBinding="Button.Click=:Container.HandlePresenterEvent Button.CheckState:=:Container.ContentVisibleFlag"
                                />
        </CATAfrVidTopBarMenu.PresenterCtl>


        <CATAfrVidTopBarMenu.PresenterInHoverMenuCtl>
          <CATVidCtlButtonIcon syp:name="HomeIcon" syp:Template="TopBarButtonNoSpace" Name="HomeIconHoverMenu"
            IconDimension="44 44"
            RequestedMinimumDimension="44 44"
            ClickMode="0"
            CheckMode="1"
            OffTexture="Large/IP_TopBarHome.png"
            GiveFocusOnDragFlag="1"
            AcceptFocusOnDropFlag="1"
            CropPolicy="CropWithDots"
            ForegroundColor="239 239 239 230"
                            
                        />
        </CATAfrVidTopBarMenu.PresenterInHoverMenuCtl>
      </CATAfrVidTopBarMenu>

    </CATAfrSYPContainerViewMetaData>
  </DataTemplate>
  

  <!--==========================================================================================-->
  <!--=============================     ME MENU        =========================================-->
  <!--==========================================================================================-->

  <DataTemplate syp:name="CATCAfrMenu_AfrTopBarMeMenu" >
    <CATAfrSYPContainerViewMetaData syp:name="MetaData" NameBis="@Name"  IconBoxFlag="1" NormalIcon="@Icon" >
      <CATAfrVidTopBarMenu syp:name="Container" syp:Template="Custom"
                           RecordName="@Name"
                           Title="@Title"
                           ContentVisibleFlag="@ContentVisibleFlag Mode=TwoWays"
                           VisibleFlag="@UsefulFlag" LayoutWhenUnvisibleFlag="0"
                           ContentShown="@HandleEventMenuDeployed"
                           PresenterClick="@HandleActionEvent"
                           PresenterTooltip="@Tooltip">

        <CATAfrVidTopBarMenu.PresenterCtl>
          <CATAfrVidExtraViewPresenter syp:name="ExtraViewPresenter" ExtraViewDataTemplate="AfrTopBarMeMenu_CID_Frame"
                                      PropertiesBinding="Button.RightClick=:Container.HandleHideEvent Button.Click=:Container.HandlePresenterEvent Button.CheckState:=:Container.ContentVisibleFlag Button.OnTexture=:Icon.OffTexture Button.IconPixelImage=:Icon.IconPixelImage"
                                />
        </CATAfrVidTopBarMenu.PresenterCtl>
        <CATAfrVidTopBarMenu.PresenterInHoverMenuCtl>
          <CATVidCtlButtonIcon syp:name="Icon" syp:Template="TopBarMeButtonNoSpace" Name="IconHoverMenu"
                    IconDimension="44 44"
                    RequestedMinimumDimension="44 44"
                    ClickMode="0"
                    CheckMode="1"
                    OffTexture="Large/IP_TopBarProfile.png"
                    GiveFocusOnDragFlag="1"
                    AcceptFocusOnDropFlag="1"
                    CropPolicy="CropWithDots"
                    ForegroundColor="239 239 239 230"                    
                                     />
        </CATAfrVidTopBarMenu.PresenterInHoverMenuCtl>
      </CATAfrVidTopBarMenu>     
    </CATAfrSYPContainerViewMetaData>
  </DataTemplate>

