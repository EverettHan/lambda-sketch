  <!--=============================     HOME MENU ITEM      =========================================-->
  <!--===============================================================================================-->

  <DataTemplate syp:name="CATCAfrMenu_AfrHomeMenuRep">
    <!--Bind the following properties-->
    <!--Name of Menu -> Name of Pen Icon (for record issue)-->
    <!--ActivatedFlag  of Menu <-> Check State of Pen Icon (useful to know if the command is running or not)-->
    <!--Click event of Pen Icon -> HandleActionEvent of Menu (trigger the command execution) -->
    <!--Extra Property AccessRole of Menu Header -> Label of AccessRole GPLabel-->
    <!--Extra Property Organization of Menu Header -> Label of Organization GPLabel-->
    <CATVidLayLayer>
      <CATAfrVidExtraViewPresenter  syp:name="ExtraViewPresenter" ExtraViewDataTemplate="HomeMenuItemWithIcon"
                                                
    PropertiesBinding=" PenIcon.Click=:HandleActionEvent PenIcon.Tooltip=:Tooltip"/>
      <!--In odt mode, some items are append to this menu, this menu should not contains items in normal mode
      so create an invisible layer which will be the container thanks to the tag |syp:name="Container" | -->
      <CATVidLayLayer syp:name="Container" VisibleFlag="0" LayoutWhenUnvisibleFlag="0"/>
    </CATVidLayLayer>

  </DataTemplate>

