  <Template syp:name="CAfrComponent" Target="CATAfrVidCtlTreeView" >
    <CATVidCtlTreeView syp:name="TreeView"
                       syp:Template="Default"
                       MouseScrollFlag="1"
                       InertiaFlag="1"
                       Indentation="0"
                       UniformItemSizes="0"
                       KeyboardManipulationFlag="1"
                       RootChildrenBackgroundVisibleFlag="1"
