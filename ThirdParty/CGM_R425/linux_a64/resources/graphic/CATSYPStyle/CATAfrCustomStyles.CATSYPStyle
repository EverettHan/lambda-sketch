  <Template syp:name="Default" Target="CATAfrVidPolygonSide">
    <CATVidLayLayer>
      <CATVidGPLine Color="@Color"
                    UseManipulatorFlag="@UseManipulatorFlagOnPoint"
                    VisibleFlag="@VisibleLineFlag"
                    FirstPoint="@InitialPoint"
                    SecondPoint="@FinalPoint"
                    OutlineColor="@OutlineColor"
                    LineType="@LineType"
                    Offset="@Offset"
                    OutlineLineType="@OutlineLineType"
                    DesaturatedFlag="@DesaturatedFlag"
                    Entry="@HandleEvent"
                    Leave="@HandleEvent"/>
      <CATVidGPLine Color="@Color"
                    UseManipulatorFlag="0"
                    VisibleFlag="@VisibleLineToCenterFlag"
                    FirstPoint="@InitialPoint"
                    SecondPoint="@Center"
                    OutlineColor="@OutlineColor"
                    LineType="@LineType"
                    Offset="@Offset"
                    OutlineLineType="@OutlineLineType"
                    DesaturatedFlag="@DesaturatedFlag"
                    LayoutWhenUnvisibleFlag="0"/>
      <CATVidLaySideAttach>
        <CATVidLayLayer CATVidLaySideAttach.IndexedConstraints="@PointAttachConstraints">
           <CATVidCtlButtonIcon syp:Template="ColorizedIcon"
                             syp:name="Point"
                             Dimension="@PointDimension Mode=OneWayToSource"
                             RequestedMinimumDimension="24 24"
                             
                             OffTexture="Spider/spider_chart_point.png"
                             OnTexture="Spider/spider_chart_point.png"
                             OverTexture="Spider/spider_chart_point.png"
                             Tooltip="@Tooltip"
                             ColorizeIconBlackPixelTo="@Color"
                             VisibleFlag="@PointVisibleFlag"
                             AlphaFilterValue="@TextureAlphaPoint"
                             Entry="@HandleEvent"
                             Leave="@HandleEvent"
                             Press="@HandleEvent"
                             Release="@HandleEvent"
                             />
            <CATVidGPSquare syp:name="ErrorPopup" Texture="Spider/spider_chart_point.png" ColorizeBlackPixelTo="250 0 0 200"  RequestedMinimumDimension="24 24"/>
        </CATVidLayLayer>
       
        <!--GridWeight="row1=1 col1=1"Color=@LabelColorRequestedMinimumDimension="@MaximumLabelDimension"RequestedMinimumDimension="@RequestedLabelDimension"RequestedMinimumDimension="@MaximumLabelDimension"-->
        
        <!--<CATVidLayGrid Color="0 100 100 100" VisibleFlag="@VisibleLabelFlag"   CATVidLaySideAttach.IndexedConstraints="@LabelAttachConstraints">
          <CATVidCtl TooltipShortHelp="@Tooltip" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
            <CATVidGPText syp:name="Label"
                 WordWrapFlag="1" 
                 WordWrapPolicy="Threshold"
                 WordWrapThreshold="@MaximumLabelHeight"
                 UseManipulatorFlag="@UseManipulatorFlagOnLabel"
                 Underline="@UseManipulatorFlagOnLabel"
                 Dimension="@LabelDimension Mode=OneWayToSource"
                 Text="@Label"
                 RequestedMinimumDimension="@MaximumLabelDimension"
                 Entry="@HandleEvent"
                 Leave="@HandleEvent"
                 Release="@HandleEvent"
                             />

          </CATVidCtl>      

          --><!--<CATVidGPSquare  CATVidLayGrid.IndexedConstraints="x=1 y=0 ySpan=2 attach=RLTB"/>
          <CATVidGPSquare  CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>--><!--
        </CATVidLayGrid>-->
          <CATVidCtl TooltipShortHelp="@Tooltip" VisibleFlag="@VisibleLabelFlag"   CATVidLaySideAttach.IndexedConstraints="@LabelAttachConstraints">
            
            <CATVidLayLayer>
              <CATVidCtlXRect syp:Template="HandleCompass" CursorShape="NorthHandCursor" FocusableFlag="0" VisibleFlag="@UseManipulatorFlagOnLabel"  Entry="@HandleEvent" Leave="@HandleEvent" Release="@HandleEvent"/>
              <CATVidGPText syp:name="Label"
                 WordWrapFlag="1" 
                 WordWrapPolicy="Threshold"
                 WordWrapThreshold="@MaximumLabelHeight"
              
                 Underline="@UseManipulatorFlagOnLabel" 
                 Dimension="@LabelDimension Mode=OneWayToSource"
                 Text="@Label"
                 RequestedMinimumDimension="@MaximumLabelDimension"
                 Entry="@HandleEvent"
                 Leave="@HandleEvent"
                 Release="@HandleEvent"
                             />
            </CATVidLayLayer>
           

          </CATVidCtl>



      </CATVidLaySideAttach>
    </CATVidLayLayer>

    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="ErrorOnPercentValueFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="ErrorPopup" Value="1"/>
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:6">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" Duration="0:0:1" From="250" To="0" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" BeginTime="0:0:1" Duration="0:0:1" From="0" To="250" />            
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" BeginTime="0:0:2" Duration="0:0:1" From="250" To="0" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" BeginTime="0:0:3" Duration="0:0:1" From="0" To="250" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" BeginTime="0:0:4" Duration="0:0:1" From="250" To="0" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="ErrorPopup" BeginTime="0:0:5" Duration="0:0:1" From="0" To="250" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>

      </CATSYPTrigger>
      <CATSYPTrigger Property="ErrorOnPercentValueFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="ErrorPopup" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="UseManipulatorFlagOnLabel" Value="0">
        <CATSYPSetter Property="Color" TargetName="Label" Value="0.8 255"/>
      </CATSYPTrigger>      
      <CATSYPTrigger Property="UseManipulatorFlagOnLabel" Value="1">
        <CATSYPSetter Property="Color" TargetName="Label" Value="0 0 255 255"/>
      </CATSYPTrigger>            
    </CATSYPTriggerCollection>

  </Template>
  <Template syp:name="Default" Target="CATAfrVidSeriesHandler">
    <CATVidLayGrid>
          <CATVidCtlCheckBox syp:Template="Default" 
                       CheckState="@VisibleSeriesState Mode=TwoWays" 
                       Label="@Label" 
                       Dimension="@LabelDimension Mode=OneWayToSource"
                       TooltipShortHelp="@Tooltip" 
                       CropPolicy="@CropPolicy"
                       ForegroundColor="@Color"
                       CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=L"/>
    </CATVidLayGrid>

  </Template>
  
  
  
