  <Template syp:name="Default" Target="CATAfrVidCtlBarChart">
    <CATVidLayGrid GridWeight="row0=1 row2=1">
      <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
      <!--Border syp:Template="ThinBorder"BackgroundColor="0.2 250"-->
      <CATVidLayStack syp:name="CategoriesCollection" Orientation="@Orientation" CellBorder="5" StackAttachmentAppendPolicy="StretchAttachmentPolicy" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
      <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=2 attach=RLTB"/>
    </CATVidLayGrid>
    
  </Template>

  <Template syp:name="Default" Target="CATAfrVidCtlBarChartCategory">
    <CATVidLayGrid GridWeight="col0=1 col1=3">
      
      <CATVidCtl TooltipShortHelp="@Tooltip" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
        <CATVidLayGrid  GridWeight="row1=1">
          <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" RequestedMinimumDimension="0 5"/>
          <CATVidGPLabel syp:name="Label" CropPolicy="CropWithDots"  
                          UseManipulatorFlag="@UseManipulatorFlagOnLabel" 
             UnderlineFlag="@UseManipulatorFlagOnLabel" RequestedMinimumDimension="50 16"
                          TextHeight="16" VerticalAlignment="1"  
                          Text="@Label"  Entry="@HandleEvent"
                 Leave="@HandleEvent"
                 Release="@HandleEvent" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
           <!--<CATVidGPText syp:name="Label"
             WordWrapFlag="1"
             UseManipulatorFlag="@UseManipulatorFlagOnLabel"
             Underline="@UseManipulatorFlagOnLabel"
                      RequestedMinimumDimension="50 16"
                         Color="@Color"
             Text="@Label"
             Entry="@HandleEvent"
             Leave="@HandleEvent"
             Release="@HandleEvent"
                         CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"
                             />-->
        </CATVidLayGrid>
       

      </CATVidCtl>
      <CATVidLayStack Items="@Items Mode=OneWayToSource" StackAttachmentAppendPolicy="StretchAttachmentPolicy"  CellBorder="3" Orientation="@Orientation" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
    </CATVidLayGrid>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="UseManipulatorFlagOnLabel" Value="0">
        <CATSYPSetter Property="TextColor" TargetName="Label" Value="0.8 255"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="UseManipulatorFlagOnLabel" Value="1">
        <CATSYPSetter Property="TextColor" TargetName="Label" Value="0 0 255 255"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>

  <Template syp:name="Default" Target="CATAfrVidCtlBarChartValue">
    <CATVidLayGrid  GridWeight="col3=1" >
      <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" RequestedMinimumDimension="10 0"/>
      <CATVidLayGrid syp:name="ExtraLabel" LayoutWhenUnvisibleFlag="0" RequestedMinimumDimension="30 16" VisibleFlag="1" GridWeight="col3=1" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB">
        <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
        <CATVidGPLabel  CropPolicy="NoCrop"  TextHeight="16" VerticalAlignment="1" TextColor="0.8 255" Text="@StrPercentValue"  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=R"/>
      </CATVidLayGrid>
      <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB" RequestedMinimumDimension="10 0"/>
      <!--<CATAfrBarChartCtl syp:Template="ButtonBar" syp:name="BarChart" ButtonSize="16 16" LayoutWhenUnvisibleFlag="0" VisibleFlag="0" PercentValue="@PercentValue" ColorBar="@Color" CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=R"/>-->
      <CATVidCtlProgressBar syp:Template="Default" syp:name="BarChart" RequestedMinimumDimension="160 16"
                            
                            VisibleFlag="1" 
                            MinimumValue="0" MaximumValue="100" Value="@PercentValue" 
                            ForegroundColor="@Color"  CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=RL"/>
    </CATVidLayGrid>

  </Template>

  <Template syp:name="Default" Target="CATAfrVidCtlScrollViews">
    <!--<CATVidLayGrid>  CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB" Scroll="@ScrollCB"-->
    <CATVidLayLayer>
      <CATVidCtlScrollableArea syp:Template="NoScrollbars" syp:name="Scrollable" VScrollbarPolicy="AlwaysHidden" VStretchPolicy="Stretch" HStretchPolicy="Stretch" InertiaFlag="0" MouseScrollFlag="1" >
        <CATVidLayGrid GridWeight="col1=1">
          <CATVidLayGrid syp:name="Stack" GridAppendPolicy="2" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" />
          <CATVidLayGrid  CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"  LayoutWhenUnvisibleFlag="0" VisibleFlag="@OnVisibleProgressTask">
            <!--<CATVidSpacer RequestedMinimumDimension="@SpaceBeforeProgressBar" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=LTB"/>-->
            <CATVidGPSquare syp:name="ProgressSquare" Texture="I_Loading_Wheel.mng" LayoutWhenUnvisibleFlag="0"  RequestedMinimumDimension="48 48" CATVidLayGrid.IndexedConstraints="x=1 y=0 " />
          </CATVidLayGrid>
          
        </CATVidLayGrid>
      </CATVidCtlScrollableArea>
      <CATVidLaySideAttach syp:name="ArrowsLayer" >
        <CATVidGPSquare syp:name="Left"  UseManipulatorFlag="1" Texture="Spider/ScrollArrowLeft.png" RequestedMinimumDimension="32 32"  TextureAlpha="255" CATVidLaySideAttach.IndexedConstraints="leftPx=0 topPercent=50" Release="@GoToLeftCB"/>
        <CATVidGPSquare syp:name="Right" UseManipulatorFlag="1" Texture="Spider/ScrollArrowRight.png" RequestedMinimumDimension="32 32"  TextureAlpha="255" CATVidLaySideAttach.IndexedConstraints="rightPx=0 topPercent=50" Release="@GoToRightCB"/>
      </CATVidLaySideAttach>
    </CATVidLayLayer>
      
    <!--</CATVidLayGrid>-->
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="LastViewFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="Right"  Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="LastViewFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="Right"  Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="FirstViewFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="Left"  Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="FirstViewFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="Left"  Value="0"/>
      </CATSYPTrigger>
      <CATSYPEventTrigger RoutedEvent="Leave" TargetName="Left">
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:2">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Left" BeginTime="0:0:1" Duration="0:0:1" From="255" To="80" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Right" BeginTime="0:0:1" Duration="0:0:1" From="255" To="80" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPEventTrigger>
      <CATSYPEventTrigger RoutedEvent="Leave" TargetName="Right">
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:2">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Left" BeginTime="0:0:1" Duration="0:0:1" From="255" To="80" />
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="TextureAlpha" CATSYPStoryboard.TargetName="Right" BeginTime="0:0:1" Duration="0:0:1" From="255" To="80" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPEventTrigger>
      <CATSYPEventTrigger RoutedEvent="Entry" TargetName="Right">
        <CATSYPSetter Property="TextureAlpha" TargetName="Left" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="Right" Value="255"/>        
      </CATSYPEventTrigger>
      <CATSYPEventTrigger RoutedEvent="Entry" TargetName="Left">
        <CATSYPSetter Property="TextureAlpha" TargetName="Left" Value="255"/>
        <CATSYPSetter Property="TextureAlpha" TargetName="Right" Value="255"/>        
      </CATSYPEventTrigger>      
    </CATSYPTriggerCollection>
  </Template>

