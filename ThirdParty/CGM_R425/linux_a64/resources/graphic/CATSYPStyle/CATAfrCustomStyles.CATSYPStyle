                      Name="SquareSelection"
                      UseManipulatorFlag="1"
                      RequestedMinimumDimension = "@RequestedMinimumDimension"
                      Press="@HandleEvent"/>
    </CATVidLayLayer>
  </Template>
  
  <Template syp:name="Default_Over" Target="CATAfrVidCtlForThumbnailOver">
    <CATVidCtlScrollableArea syp:Template="NoScrollbars"
                             syp:name="TextContent"
                             Name="TextContent"
                             VScrollbarPolicy="AlwaysHidden"
                             HScrollbarPolicy="AlwaysHidden"
                             InertiaFlag="0"
                             MouseScrollFlag="0"
                             UseViewportFilter="1">
        <CATVidLayLayer>
          <CATVidLayLayer syp:name="LayerTextLabel"
                          VisibleFlag="1">
            <CATVidGPText syp:name="TextLabel"
                          RequestedMinimumDimension = "@RequestedMinimumDimension"
                          Text = "@Display"
                          FontHeight = "14"
                          WordWrapFlag="1"
                          WordWrapPolicy="Auto"
                          Color="65 110 138 255"
                          VisibleFlag="@VisibleFlag"
                          UseManipulatorFlag="0"
                          FontFamily="3ds"/>
          </CATVidLayLayer>
            <CATVidLayLayer syp:name="LayerTextEditor"
                            VisibleFlag="0">
              <CATVidCtlTextEditor syp:Template="TextEditor"
                                   syp:name="TextEditor"
                                   BackgroundColor="0 255"
                                   Text="@Display"
                                   EditingFinished="@HandleEditingFinishedEvent"
                                   VScrollbarPolicy="AlwaysHidden"
                                   HScrollbarPolicy="AlwaysHidden"
                                   InertiaFlag="0"
                                   MouseScrollFlag="0"
                                   RequestedMinimumDimension="@RequestedMinimumDimension"/>
          </CATVidLayLayer>
        </CATVidLayLayer>
      </CATVidCtlScrollableArea>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="ActivateTextFlag" Value="0">
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="MinimumViewportHeight" CATSYPStoryboard.TargetName="TextContent" Duration="0:0:0.2" To="21" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ActivateTextFlag" Value="1">
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="MinimumViewportHeight" CATSYPStoryboard.TargetName="TextContent" Duration="0:0:0.2" To="65" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ActivateTextFlag" Value="3">
        <CATSYPBeginStoryboard>
          <CATSYPStoryboard Duration="0:0:0.2">
            <CATSYPIntAnimation CATSYPStoryboard.TargetProperty="MinimumViewportHeight" CATSYPStoryboard.TargetName="TextContent" Duration="0:0:0.2" To="40" />
          </CATSYPStoryboard>
        </CATSYPBeginStoryboard>
      </CATSYPTrigger>
      <CATSYPTrigger Property="EditorFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="LayerTextEditor" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="LayerTextLabel" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="EditorFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="LayerTextEditor" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="LayerTextLabel" Value="1"/>
        <CATSYPSetter Property="MinimumViewportHeight" TargetName="TextContent" Value="21"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>

