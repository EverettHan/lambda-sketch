      <CATSYPTrigger Property="ScrollableFlag" Value="0">
        <CATSYPSetter Property="HStretchPolicy" TargetName="ListView" Value="StretchFit"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ScrollableFlag" Value="0">
        <CATSYPSetter Property="VStretchPolicy" TargetName="ListView" Value="StretchFit"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ScrollableFlag" Value="1">
        <CATSYPSetter Property="HStretchPolicy" TargetName="ListView" Value="Stretch"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ScrollableFlag" Value="1">
        <CATSYPSetter Property="VStretchPolicy" TargetName="ListView" Value="Stretch"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </Template>

