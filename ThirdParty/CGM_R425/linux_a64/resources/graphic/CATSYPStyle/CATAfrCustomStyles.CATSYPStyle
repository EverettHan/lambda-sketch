                       LayoutWhenUnvisibleFlag="0"
                       UniformItemSizes="1"
                       ItemsFlow="@ItemsFlow"
                       ViewMode="@ViewMode"
                       KeyboardManipulationFlag="1"
                       TooltipProvider="@TooltipProvider"
                       PreHighlightBorderColor="13 97 150 200"
                       PreHighlightColor="13 97 150 200"
                       SelectionBorderColor="13 97 150 255"
                       SelectionColor="13 97 150 255"
                       CurrentItemBorderColor="13 97 150 255"
                       ShortTooltipTemplateName="@ShortTooltipTemplateName"
                       ShortTooltipStyleSheet="@ShortTooltipStyleSheet"
                       ItemsScrollMode="@ItemsScrollMode"
                       TooltipWrapPolicy="@TooltipWrapPolicy"/>
