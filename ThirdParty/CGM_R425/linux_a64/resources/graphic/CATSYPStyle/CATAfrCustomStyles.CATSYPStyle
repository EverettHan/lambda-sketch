  <Template syp:name="Default" Target="CATAfrVidCtlForThumbnail">
    <CATVidLayLayer VisibleFlag="@VisibleFlag"
                    LayoutWhenUnvisibleFlag="@LayoutWhenUnvisibleFlag">
      <CATAfrVidCtlForThumbnailOver syp:name="Thumbnail"
                                    syp:Template="Default"
                                    RequestedMinimumDimension = "@RequestedMinimumDimension"
                                    Display = "@Display"
                                    FontHeight = "@FontHeight"
                                    EditorFlag = "@EditorFlag Mode=TwoWays"
                                    ActivateTextFlag = "@ActivateTextFlag Mode=TwoWays"
                                    VisibleFlag="@VisibleFlag"/>
