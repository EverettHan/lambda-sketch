<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">


  <Template syp:name="Default" Target="CATVidHit">
    <CATVidLayLayer syp:name="CATVidHitWidgetsParentLayer" Name="CATVidHitWidgetsParentLayer">
      <CATVidLayLayer syp:name="CATVidHitWidgetsContainerLayer" Name="CATVidHitWidgetsContainerLayer"/>
      <CATVidGPSquare syp:name="CATVidHitWidgetsSquare" Name="CATVidHitWidgetsSquare" UseManipulatorFlag="1" 
                      Press="@HandlePressNotification"
                      DoubleClick="@HandleDoubleClickNotification"
                      RightClick="@HandleRightClickNotification"
                      EndContext="@HandleEndContextNotification"
                      Drag="@HandleDragNotification"
                      MoveOver="@HandleMoveOverNotification"
                      Leave="@HandleLeaveNotification"
                      Entry="@HandleEntryNotification"
                      Release="@HandleReleaseNotification"
                      MultiplePress="@HandleMultiplePressNotification"
                 
                      />
    </CATVidLayLayer>
  </Template>


  
