          <CATVidGPSquare syp:name="I_CompassWestButton" Name="I_CompassWestButton"
                        Texture="Compass/West/I_CompassWestButton.png"/>
          <CATVidGPSquare syp:name="IF_CompassWestButton" Name="IF_CompassWestButton"
