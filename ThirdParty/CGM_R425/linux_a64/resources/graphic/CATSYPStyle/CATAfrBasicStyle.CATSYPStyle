  <DataTemplate syp:name="DefaultAfrLongTooltip">

    <CATVidLayLayer>
      <!-- Tooltip : Pickable background, needed to receive OnMove, ... GP events -->
      <CATVidGPSquare Name="Background" PickFlag="1" UseManipulatorFlag="1"/>

      <!-- Tooltip : Foreground -->
      <CATVidLayGrid>

        <!-- Title area -->
        <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" syp:name="TitleArea" LayoutWhenUnvisibleFlag="0">
          <!-- Title area : Background -->
          <CATVidGPSmartSquare PickFlag="1" GridWeight="col1=1 row1=1" Texture="VidTooltipTitle.png" >
            <CATVidSpacer RequestedMinimumDimension="2 2"  CATVidGPSmartSquare.GPConstraints="x=0 y=0 attach=RLTB"/>
            <CATVidSpacer RequestedMinimumDimension="2 1" CATVidGPSmartSquare.GPConstraints="x=1 y=1 attach=RLTB"/>
            <CATVidSpacer RequestedMinimumDimension="2 0" CATVidGPSmartSquare.GPConstraints="x=2 y=2 attach=RLTB"/>
          </CATVidGPSmartSquare>
          <!-- Title area : Foreground -->
          <CATVidLayGrid CellSpacing="4">
            <CATVidGPText
                Text="@Title"
                Bold="1"
                Color="80 80 80 255"
                LeftMargin="9"
                RightMargin="9"
                TopMargin="3"
                BottomMargin="3"
                Interline="0"
                WordWrapFlag="1"
                WordWrapPolicy="ToleranceMargin"
                WordWrapThreshold="@WrapThreshold"
                WordWrapToleranceMargin="@WrapToleranceMargin"
                OptimizeMinimumDimensionFlag="1"
                CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB"/>
          </CATVidLayGrid>
        </CATVidLayLayer>

        <!-- Long help area -->
        <CATVidLayGrid syp:name="LongHelpArea"
                       CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"
                       LayoutWhenUnvisibleFlag="0"
                       GridWeight="row0=1"
                       CellSpacing="4">
          <CATVidGPText
            Color="80 80 80 255"
            Text="@LongHelp"
            LeftMargin="0"
            RightMargin="0"
            TopMargin="0"
            BottomMargin="0"
            Interline="0"
            WordWrapFlag="1"
            WordWrapPolicy="ToleranceMargin"
            WordWrapThreshold="@WrapThreshold"
            WordWrapToleranceMargin="@WrapToleranceMargin"
            OptimizeMinimumDimensionFlag="1"
            CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
        </CATVidLayGrid>

        <!--  Image area -->
        <CATVidLayGrid syp:name="ImageArea"
                       LayoutWhenUnvisibleFlag="0"
                       CATVidLayGrid.IndexedConstraints="x=0 y=2 attach=RLTB"
                       GridWeight="row0=1"
                       CellSpacing="4">
          <CATVidGPSquare CATVidLayGrid.IndexedConstraints="x=0 y=0"
                          Texture="@Image"
                          RequestedMinimumDimension="@ImageDimension"
                          syp:name="ImageSquare"/>
          <CATVidSpacer RequestedMinimumDimension="0 5" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
        </CATVidLayGrid>

        <!--  Hyperlink area -->
        <CATVidLayGrid syp:name="HyperlinkArea"
                       LayoutWhenUnvisibleFlag="0"
                       CATVidLayGrid.IndexedConstraints="x=0 y=3 attach=RLTB" GridWeight="col1=1"
                       CellSpacing="4">
          <CATVidGPSquare Texture="I_TooltipLonghelp.png" RequestedMinimumDimension="16 16" CATVidLayGrid.IndexedConstraints="x=0 y=0"/>
          <CATVidGPLabel Name="Hyperlink" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLB" TextHeight="12"
                         Text="@HyperlinkLabel"  CropPolicy="NoCrop" TextColor="80 80 80 255"
                          LayoutWhenUnvisibleFlag="0" />
        </CATVidLayGrid>
      </CATVidLayGrid>
    </CATVidLayLayer>

    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Title" Value="">
        <CATSYPSetter Property="VisibleFlag" TargetName="TitleArea" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="Image" Value="">
        <CATSYPSetter Property="VisibleFlag" TargetName="ImageArea" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="LongHelp" Value="">
        <CATSYPSetter Property="VisibleFlag" TargetName="LongHelpArea" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="HyperlinkLabel" Value="">
        <CATSYPSetter Property="VisibleFlag" TargetName="HyperlinkArea" Value="0"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
