<?xml version="1.0"?>
<Styles xmlns:syp = "http://www.3ds.com/xsd/SYP">
  <Resources>
    <ResourceDictionary Source="basicTweakers.sypstyle"/>
  </Resources>

  <!--===================================-->
  <!--Here you define your components view -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrForm_AfrFormUndef">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    IconName="@IconName"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility"
                    EnableFlag="@EnableFlag"
                    LayoutWhenUnvisibleFlag="0">
      <CATVidCtlForm syp:Template="Form"
                     syp:name="Container"
                     GridFlag="@GridFlag"
                     NlsFiles="@ResourceFile"
                     AlternateBackgroundFlag="@AlternateBackgroundFlag"
                     LabelsCropPolicy ="@LabelsCropPolicy"
                     UndoButtonFlag="0"
                     Data="@IntroObject"
                     RequestedMinimumDimension="@MinimumDimension"
                     EditingStarted="@OnEditingStarted"
                     EditingFinished="@OnEditingFinished"
                     ValueLiveChanged="@OnValueLiveChanged"
                     ValueChanged="@OnValueChanged"
                     UseTransactionFlag="@UseTransactionFlag"
                     UseDetailedViewTransactionFlag="@UseDetailedViewTransactionFlag"
                     TooltipProvider="@TooltipProvider"
                     TooltipWrapPolicy="Multiline"
                     ShortTooltipTemplateName="DefaultShortTooltip"
                     ShortTooltipStyleSheet="AfrComponentViews/Form.CATSYPStyle"/>
    </CATAfrSmartCtl>
  </DataTemplate>

  <!--===================================-->
  <!-- Form in center -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrForm_AfrFormCenter">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    IconName="@IconName"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility"
                    EnableFlag="@EnableFlag"
                    LayoutWhenUnvisibleFlag="0">
    <CATVidLayGrid GridWeight="row1=1 row3=1">
      <!-- Form in center car le form vid met un spacer de 4 4 Ã  la fin -->
      <CATVidGPSquare RequestedMinimumDimension="4 8"
                      CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
      <CATVidGPSquare CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
      <CATVidCtlForm syp:Template="Form"
                     syp:name="Container"
                     GridFlag="@GridFlag"
                     NlsFiles="@ResourceFile"
                     AlternateBackgroundFlag="@AlternateBackgroundFlag"
                     LabelsCropPolicy ="@LabelsCropPolicy"
                     UndoButtonFlag="0"
                     Data="@IntroObject"
                     RequestedMinimumDimension="@MinimumDimension"
                     EditingStarted="@OnEditingStarted"
                     EditingFinished="@OnEditingFinished"
                     ValueLiveChanged="@OnValueLiveChanged"
                     ValueChanged="@OnValueChanged"
                     UseTransactionFlag="@UseTransactionFlag"
                     UseDetailedViewTransactionFlag="@UseDetailedViewTransactionFlag"
                     TooltipProvider="@TooltipProvider"
                     TooltipWrapPolicy="Multiline"
                     ShortTooltipTemplateName="DefaultShortTooltip"
                     ShortTooltipStyleSheet="AfrComponentViews/Form.CATSYPStyle"
                     CATVidLayGrid.IndexedConstraints="x=0 y=2 attach=RLTB"/>
        <CATVidGPSquare CATVidLayGrid.IndexedConstraints="x=0 y=3 attach=RLTB"/>
      </CATVidLayGrid>
    </CATAfrSmartCtl>
  </DataTemplate>
  
  <DataTemplate syp:name="CATCAfrForm_Undef">
    <CATVidCtl Name="@Name"
               syp:name="Container"
               VisibleFlag="@Visibility"
               ChildrenLifeCyclePolicy="DontDestroyChildren"
               EnableFlag="@EnableFlag"
               LayoutWhenUnvisibleFlag="0"
               RequestedMinimumDimension="@MinimumDimension">
    </CATVidCtl>
  </DataTemplate>

  <!--===================================-->
  <!--Form with style Expander -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrForm_AfrFormExpander" syp:version="2">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    ContentPanelVisibleFlag="1"
                    ResizableDialogBoxFlag="1"
                    CompactDialogBoxFlag="1"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility"
                    EnableFlag="@EnableFlag"
                    LayoutWhenUnvisibleFlag="0">
      <CATVidLayGrid GridWeight="row0=1" CellSpacing="1">
        <CATVidCtlExpander syp:Template="Divided"
                         ExpandedFlag="@DecoratorExpandFlag Mode=TwoWays"
                         LayoutWhenUnvisibleFlag="0"
                         FontWeight="0"
                         Label="@Title"
                         BackgroundColor="0.05 200"
                         ForegroundColor="1 255"
                         VisibleFlag="@Visibility"
                         Icon="@IconName"
                         IconDimension="16 16"
                         Collapsed ="@ExpanderEvent"
                         Expanded ="@ExpanderEvent"
                         CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
          <CATVidCtlForm syp:Template="Form"
                         GridFlag="@GridFlag"
                         NlsFiles="@ResourceFile"
                         AlternateBackgroundFlag="@AlternateBackgroundFlag"
                         LabelsCropPolicy ="@LabelsCropPolicy"
                         UndoButtonFlag="0"
                         syp:name="Container"
                         Data="@IntroObject"
                         RequestedMinimumDimension="@MinimumDimension"
                         EditingStarted="@OnEditingStarted"
                         EditingFinished="@OnEditingFinished"
                         ValueLiveChanged="@OnValueLiveChanged"
                         ValueChanged="@OnValueChanged"
                         UseTransactionFlag="@UseTransactionFlag"
                         UseDetailedViewTransactionFlag="@UseDetailedViewTransactionFlag"
                         TooltipProvider="@TooltipProvider"
                         TooltipWrapPolicy="Multiline"
                         ShortTooltipTemplateName="DefaultShortTooltip"
                         ShortTooltipStyleSheet="AfrComponentViews/Form.CATSYPStyle"/>
        </CATVidCtlExpander>
        <CATVidGPSquare CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
      </CATVidLayGrid>
    </CATAfrSmartCtl>
  </DataTemplate>

