                          Orientation="Vertical"
                          CellBorder="1"
                          StackAttachmentAppendPolicy="StretchAttachmentPolicy"/>
      </CATVidCtlScrollableArea >
      </CATVidLayGrid>
    </CATAfrSmartCtl>
