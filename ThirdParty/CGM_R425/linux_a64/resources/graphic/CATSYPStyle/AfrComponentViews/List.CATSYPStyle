  <!--===================================-->
  <!--In combo representation type-->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrList_Combo">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    ContentPanelVisibleFlag="1"
                    ResizableDialogBoxFlag="1"
                    CompactDialogBoxFlag="1"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility"
                    LayoutWhenUnvisibleFlag="0"
                    EnableFlag="@EnableFlag">
      <CATAfrVidCtlComboView syp:name="Container"
                             syp:Template="Default"
                             Title="@Title"
                             SourceDataTemplateName="CATCAfrList_ListIconText"
                             SourceStyleSheet="CATAfrComponentViews.CATSYPStyle"
                             TooltipProvider="@TooltipProvider"
                             ShortTooltipTemplateName="@ShortTooltipTemplateName"
                             ShortTooltipStyleSheet="@ShortTooltipStyleSheet"
                             ListEvent="@HandleListEvent"
                             RequestedMinimumDimension="@MinimumDimension"
                             SelectedIndexString="@ListSelectedObjectInString"
                             SelectedElements="@SelectedElements"
                             DispatchSelectionChangedEvent="@DispatchSelectionChangedEvent"/>
    </CATAfrSmartCtl>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="ThumbnailsMode" Value="NewTile">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_NewTile"/>
        <CATSYPSetter Property="ItemsFlow" TargetName="Container" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="NewClassic">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_NewClassic"/>
        <CATSYPSetter Property="ItemsFlow" TargetName="Container" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="Classic_Tiers">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Classic_Tiers"/>
        <CATSYPSetter Property="ItemsFlow" TargetName="Container" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="CAfrComponentView">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_CAfr"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="Basic">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_ListIconText"/>
        <CATSYPSetter Property="ItemsFlow" TargetName="Container" Value="0"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </DataTemplate>
  
