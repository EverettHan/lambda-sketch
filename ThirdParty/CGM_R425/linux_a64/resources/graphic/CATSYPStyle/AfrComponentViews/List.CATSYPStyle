  <DataTemplate syp:name="CATCAfrList_Classic">
 <CATAfrSmartCtl Name="@Name"
                 TitleDialogBox="@Title"
                 SnappableFlag="1"
                 ViewParent="@ViewParent"
                 CreateDialogBoxFlag="@RootFlag"
                 ContentPanelVisibleFlag="1"
                 ResizableDialogBoxFlag="1"
                 CompactDialogBoxFlag="1"
                 AutoCloseDialogBoxFlag="1"
                 DiscretionFlag="1"
                 VisibleFlag="@Visibility"
                 LayoutWhenUnvisibleFlag="0"
                 EnableFlag="@EnableFlag">
          <CATAfrVidCtlListView syp:name="Container" 
                                SourceDataTemplateName="CATCAfrList_Default" 
                                SourceStyleSheet="CATAfrComponentViews.CATSYPStyle"
                                SelectionMode="@SelectionMode" 
                                SelectedIndexString="@ListSelectedObjectInString"
                                ScrollableFlag="@ScrollableFlag"
                                DragFlag="@DragFlag"
                                DropFlag="@DropFlag"
                                ItemsFlow="0" 
                                ViewMode="1"
                                TooltipProvider="@TooltipProvider"
                                ShortTooltipTemplateName="@ShortTooltipTemplateName"
                                ShortTooltipStyleSheet="@ShortTooltipStyleSheet"
                                ListEvent="@HandleListEvent"
                                ItemsScrollMode="PixelPerPixel"
                                MaxItemsVisibles="@MaximumItemsVisible"
                                RequestedMinimumDimension="@MinimumDimension"
                                DispatchSelectionChangedEvent="@DispatchSelectionChangedEvent"
                                SelectedElements="@SelectedElements"
                                DropLocations="@DropLocationsValues"/>
    </CATAfrSmartCtl>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="ThumbnailsMode" Value="ThumbnailViewer">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Default_Viewer"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="Drafting3DMaster">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Drafting3DMaster"/>
      </CATSYPTrigger>
     <CATSYPTrigger Property="ThumbnailsMode" Value="Tile">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Tile"/>
      </CATSYPTrigger>
     <CATSYPTrigger Property="ThumbnailsMode" Value="Classic">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Default"/>
      </CATSYPTrigger>
     <CATSYPTrigger Property="ThumbnailsMode" Value="NewClassic">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_NewClassic"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="NavigationTag">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_NavigationTag"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="CAfrComponentView">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_CAfr"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="QuickPick">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_QuickPick"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailsMode" Value="Basic">
        <CATSYPSetter Property="SourceDataTemplateName" TargetName="Container" Value="CATCAfrList_Basic"/>
        <CATSYPSetter Property="ItemsFlow" TargetName="Container" Value="1"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </DataTemplate>

