                           TextColor="1 255"
                           UnderlineFlag="@UnderlineFlag"/>
          </CATVidLayLayer>
          <CATVidLayLayer syp:name="LayerTextEditor"
                          VisibleFlag="0">
            <CATVidCtlLineEditor syp:name="TextEditor"
