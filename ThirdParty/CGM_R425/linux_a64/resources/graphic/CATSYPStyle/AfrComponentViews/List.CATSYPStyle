  <!--===================================-->
  <!--Representation Menu -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrList_AfrMenu">
    <CATAfrVidViewsStack syp:name="Container" Orientation="Vertical" LayoutWhenUnvisibleFlag="0" VisibleFlag="@Visibility" />
  </DataTemplate>
  <!--===================================-->
  <!--Representation Menu With Title -->
  <!--===================================-->
      <DataTemplate syp:name="CATCAfrList_AfrMenuWithTitle">

        <CATVidLayGrid GridWeight="row1=1" VisibleFlag="@Visibility" LayoutWhenUnvisibleFlag="0">
          <CATVidLayGrid  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" >
            <CATVidSpacer RequestedMinimumDimension="1 10"  CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
            <CATVidGPLabel Text="@Title" TextColor="255 255 255 255" VisibleFlag="0" TextHeight="25" BoldFlag="1" LeftMargin="5" CropPolicy="NoCrop" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
            <CATVidSpacer RequestedMinimumDimension="1 10"  CATVidLayGrid.IndexedConstraints="x=0 y=2 attach=RLTB"/>
            <CATVidLayStack syp:name="Container" Orientation="Vertical" StackAttachmentAppendPolicy="StretchAttachmentPolicy" CATVidLayGrid.IndexedConstraints="x=0 y=3 attach=RLTB"/>
          </CATVidLayGrid>
          <CATVidGPSquare CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
        </CATVidLayGrid>


  </DataTemplate>

