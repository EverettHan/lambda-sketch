  <DataTemplate syp:name="CATCAfrChannels_AfrChannelUndef">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    ContentPanelVisibleFlag="1"
                    ResizableDialogBoxFlag="1"
                    CompactDialogBoxFlag="1"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility">
      <CATVidLayStack syp:name="Container"
                      VisibleFlag="@Visibility"
                      Orientation="Vertical"
                      StackAttachmentAppendPolicy="StretchAttachmentPolicy"/>
    </CATAfrSmartCtl>
  </DataTemplate>

  <DataTemplate syp:name="CATCAfrChannels_AfrChannelExpander">
    <CATAfrSmartCtl Name="@Name"
                    TitleDialogBox="@Title"
                    SnappableFlag="1"
                    ViewParent="@ViewParent"
                    CreateDialogBoxFlag="@RootFlag"
                    ContentPanelVisibleFlag="1"
                    ResizableDialogBoxFlag="1"
                    CompactDialogBoxFlag="1"
                    AutoCloseDialogBoxFlag="1"
                    DiscretionFlag="1"
                    VisibleFlag="@Visibility">
      <CATVidCtlExpander syp:Template="Expander"
                         ExpandedFlag="@ExpandedFlag Mode=TwoWays"
                         ExclusiveFlag="@ExclusiveFlag"
                         LayoutWhenUnvisibleFlag="0"
                         FontWeight="1"
                         Label="@Title"
                         BackgroundColor="0.05 200"
                         ForegroundColor="1 255"
                         VisibleFlag="@Visibility">
        <CATVidLayGrid>
          <CATVidSpacer RequestedMinimumDimension="1 1"
                        CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
          <CATVidLayLayer syp:name="Container"
                          VisibleFlag="@Visibility"
                          CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB"/>
          <CATVidSpacer RequestedMinimumDimension="2 2"
                        CATVidLayGrid.IndexedConstraints="x=0 y=2 attach=RLTB"/>
        </CATVidLayGrid>
      </CATVidCtlExpander>
    </CATAfrSmartCtl>
  </DataTemplate>

