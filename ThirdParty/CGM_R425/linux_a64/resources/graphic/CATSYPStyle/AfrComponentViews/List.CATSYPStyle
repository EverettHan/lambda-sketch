  <DataTemplate syp:name="CATCAfrTree_Channel">
    <CATAfrSYPAfrComponentMetaData syp:name="MetaData"
                                   CATAfrColor="@ShadingColor">
      <CATVidLayGrid GridWeight="col2=2">
