          <CATVidGPSquare syp:name="IconType"
                          RequestedMinimumDimension = "22 22"
                          Texture = "@Icon"
                          PixelImage = "@IconPixelImage"
                          VisibleFlag="0"
                          LayoutWhenUnvisibleFlag="0"
                          CATVidLayGrid.IndexedConstraints = "x=0 y=0 attach=LT"/>
          <!--<CATVidCtlTextEditor Text="@Display"
                               RequestedMinimumDimension="122 32"
                               ReadOnlyFlag="1"
                               VScrollbarPolicy="AlwaysHidden"
                               HScrollbarPolicy="AlwaysHidden"
                               BackgroundColor="0.1 0"
