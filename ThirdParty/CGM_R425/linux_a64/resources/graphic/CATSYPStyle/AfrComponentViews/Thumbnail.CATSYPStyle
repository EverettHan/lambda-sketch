        </CATVidLayLayer>
      </CATVidLayGrid>
    </CATAfrSYPAfrComponentMetaData>
   
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="ThumbnailFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="Text" Value="0"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="Image" Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="Text" Value="1"/>
        <CATSYPSetter Property="VisibleFlag" TargetName="Image" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="IconTypeFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="IconType" Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="IconTypeFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="IconType" Value="0"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </DataTemplate>

  <!--===================================-->
  <!--CATCAfrList_Default_Viewer -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrList_Default_Viewer">
    <CATAfrSYPAfrComponentMetaData syp:name="MetaData" CATAfrColor="@ShadingColor">
      <CATVidLayGrid CellSpacing = "0" Color = "1 0">
      <CATVidGPSquare RequestedMinimumDimension = "150 150"
