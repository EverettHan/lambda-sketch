                        CATVidLayGrid.IndexedConstraints="x=1 y=2 xSpan=2 ySpan=1 attach=RLTB">
          <CATVidGPSquare RequestedMinimumDimension = "144 108"
                        Texture = "@Thumbnail"
                        PixelImage = "@ThumbPixelImage"/>
        </CATVidLayLayer>
        <CATVidLayLayer syp:name="Text"
                        VisibleFlag="0"
                        CATVidLayGrid.IndexedConstraints="x=1 y=2 xSpan=2 ySpan=1 attach=RLTB">
          <CATVidGPLabel Text="@Description"
                         RequestedMinimumDimension = "144 108"
                         TextColor = "0.7 255"
                         TextHeight="16"
                         HorizontalAlignment="1"
                         VerticalAlignment="2"
                         CropPolicy="CropWithDots"
                         LeftMargin="3"
                         RightMargin="3"
                         BottomMargin="0"
                         TopMargin = "1"
