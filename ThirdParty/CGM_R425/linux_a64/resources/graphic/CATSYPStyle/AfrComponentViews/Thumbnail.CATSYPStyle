<?xml version="1.0"?>
<Styles xmlns:syp = "http://www.3ds.com/xsd/SYP">
  <Resources>
    <ResourceDictionary Source="basicTweakers.sypstyle"/>
  </Resources>

  <!--===================================-->
  <!--For Thumbails -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrList_Default_TPC">
    <CATVidCtlBorder syp:name="MainBorder" syp:Template="Shadow">
      <CATVidLayGrid Name="Layer" CellSpacing="2" GridWeight="row0=1">
        <CATVidLayLayer Name="ButtonLayer" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
          <CATVidGPSquare Color="219 224 189 150"/>
          <CATVidLayGrid Name="ButtonGrid" CellSpacing="2" GridWeight="row0=1 col0=1">
            <CATVidCtlButton syp:name="Button" syp:Template="Button"
                             CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"
                             FocusableFlag="0"
                             ForegroundColor="@ForegroundColor"
                             BackgroundColor="219 224 189 150"
                             Icon="@Thumbnail"
                             IconPixelImage="@ThumbPixelImage"
                             IconDimension="144 108"
                             Tooltip="@Display"
                             CursorShape="NorthHandCursor"/>
          </CATVidLayGrid>
        </CATVidLayLayer>
        <CATVidLayLayer Name="Layer" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB">
          <CATVidGPSquare Color="219 224 189 150" Name="PickZone" PickFlag="1" UseManipulatorFlag="1"/>
          <CATVidGPLabel Text="@Display" FontSize="16" CaptionFontFlag="@FontWeight"
                         TesselateFontFlag="@TesselateFontFlag" CropPolicy="CropWithDots"
                         TextColor="0.25 255" HorizontalAlignment="1"/>
        </CATVidLayLayer>
      </CATVidLayGrid>
    </CATVidCtlBorder>
  </DataTemplate>

  <!--===================================-->
  <!-- CATCAfrList_NewClassic -->
  <!--===================================-->
  <DataTemplate syp:name="CATCAfrList_NewClassic">
    <CATAfrSYPAfrComponentMetaData syp:name="MetaData" CATAfrColor="@ShadingColor">
      <CATVidLayLayer syp:name="LayLayerThumb">
        <CATVidGPNinePatchSquare syp:name="ContourSquare"
                                 ScalableAreaBorder="5"
                                 Texture="Thumbnail_Normal.png"
                                 LayoutWhenUnvisibleFlag="0"
                                 RequestedMinimumDimension = "156 120"/>
        <CATVidLayGrid CellSpacing = "6">
          <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
            <!-- Thumbnails -->
            <CATVidGPSquare Name="Thumbnail"
                            Texture = "@Thumbnail"
                            PixelImage = "@ThumbPixelImage"
                            AnimationOnFlag = "@LoaderFlag"/>
            <!-- Icone ranking -->
            <CATVidLaySideAttach>
              <CATVidGPSquare Name="RankingIcon"
                              RequestedMinimumDimension = "88 20"
                              PixelImage = "@ThumbDisplay"
                              CATVidLaySideAttach.IndexedConstraints="leftPx=23 topPx=2"/>
            </CATVidLaySideAttach>
            <!-- Icone type and label -->
            <CATVidLayGrid CellSpacing = "0" GridWeight="row0=1">
              <!-- Spacer pour le decalage bas -->
              <CATVidSpacer CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
              <CATAfrVidLayLayerForThumbnail syp:name="LayLayerMain"
                                             Display = "@Display"
                                             ActivateTextFlag="@ActivateTextFlag Mode=TwoWays"
                                             CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB">
                <!-- Square du fond -->
                <CATVidGPSquare syp:name="BackGroundSquare"
                                Color="255 255 255 220"
                                PickFlag="1"
                                Name="SquareDuFond"/>
                <!-- Icon type and Display -->
                <CATVidLayGrid CellSpacing = "0" GridWeight="col1=2"
                               Color = "@VidColor ElementName=MetaData"
                               RequestedMinimumDimension = "144 21">
                  <CATVidSpacer RequestedMinimumDimension = "5 5"
                                CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                  <CATVidGPSquare syp:name="IconType" Name="SquareIconType"
                                RequestedMinimumDimension = "16 16"
                                Texture = "@Icon"
                                PixelImage = "@IconPixelImage"
                                VisibleFlag="0"
                                LayoutWhenUnvisibleFlag="0"
                                CATVidLayGrid.IndexedConstraints = "x=0 y=1 attach=LT"/>
                  <CATAfrVidCtlForThumbnail Name="@Name"
                                            VisibleFlag="1"
                                            RequestedMinimumDimension = "123 21"
                                            Display = "@Display"
                                            ReadOnlyFlag="@ReadOnlyFlag"
                                            EditorFlag="@EditorFlag Mode=TwoWays"
                                            SelectedFlag="@SelectedFlag Mode=TwoWays"
                                            ActivateTextFlag="@ActivateTextFlag"
                                            EditingFinished="@HandleItemEvent"
                                            FontHeight="12"
                                            CATVidLayGrid.IndexedConstraints = "x=1 y=1 attach=RLTB">
                  </CATAfrVidCtlForThumbnail>
                </CATVidLayGrid>
              </CATAfrVidLayLayerForThumbnail>
            </CATVidLayGrid>
          </CATVidLayLayer>
        </CATVidLayGrid>
      </CATVidLayLayer>
    </CATAfrSYPAfrComponentMetaData>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="IconTypeFlag" Value="1">
        <CATSYPSetter Property="VisibleFlag" TargetName="IconType" Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="IconTypeFlag" Value="0">
        <CATSYPSetter Property="VisibleFlag" TargetName="IconType" Value="0"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="0">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_Normal.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="1">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_Over.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="2">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_Selected.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="3">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_UIActive.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="4">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_OverSelected.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="5">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_OverSUI.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="6">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_OUI.png"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="ThumbnailContour" Value="7">
        <CATSYPSetter Property="Texture" TargetName="ContourSquare" Value="Thumbnail_SUI.png"/>
      </CATSYPTrigger>
    </CATSYPTriggerCollection>
  </DataTemplate>

