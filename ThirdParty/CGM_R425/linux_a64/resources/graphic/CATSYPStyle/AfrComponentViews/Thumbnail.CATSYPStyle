      <CATVidLayGrid GridWeight="col1=1" CellSpacing = "0" Color = "1 0">
        <CATVidGPSquare RequestedMinimumDimension = "150 150"
                        Color = "0 25"
                        CATVidLayGrid.IndexedConstraints = "x=0 y=0 xSpan=5 ySpan=4"/>
        <CATVidGPSquare RequestedMinimumDimension = "144 144"
                        PickFlag = "0"
                        Color = "0 254"
                        CATVidLayGrid.IndexedConstraints = "x=1 y=1 xSpan=2 ySpan=2"/>
        <CATVidGPSquare RequestedMinimumDimension = "150 3"
                        Color = "@VidColor ElementName=MetaData"
                        CATVidLayGrid.IndexedConstraints = "x=0 y=0 xSpan=5 ySpan=1"/>
