                    TitleDialogBox             ="@Title"
                    IconName                   ="@IconName"
                    SnappableFlag              ="1"
                    DialogBoxDimension         ="@DialogBoxDimension Mode=TwoWays"
                    ViewParent                 ="@ViewParent"
                    CreateDialogBoxFlag        ="@RootFlag"
                    ContentPanelVisibleFlag    ="@ContentVisibleFlag"
                    ResizableDialogBoxFlag     ="@ResizableFlag"
                    CompactDialogBoxFlag       ="@CompactFlag"
                    VerticallyCompactDialogBoxFlag="@VerticallyCompactFlag"
                    AutoCloseDialogBoxFlag     ="@AutoCloseFlag"
