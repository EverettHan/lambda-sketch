                    PanelClosed                ="@CloseCB"
                    MinimizeCB                 ="@MinimizeCB"
                    HorizontallyStretchableFlag="@HorizontallyStretchableFlag"
                    VerticallyStretchableFlag  ="@VerticallyStretchableFlag"
