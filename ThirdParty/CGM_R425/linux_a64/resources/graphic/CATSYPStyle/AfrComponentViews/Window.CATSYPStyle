  <DataTemplate syp:name="CATCAfrWindow_AfrFraLabel">
    <CATAfrSmartCtl Name                   ="@Name"
                    SnappableFlag          ="2"
                    AutoCloseDialogBoxFlag ="0"
                    ViewParent             ="@ViewParent"
                    TitleDialogBox         ="@Title"
                    IconName               ="@IconName"
                    DialogBoxDimension     ="@DialogBoxDimension Mode=TwoWays"
                    CreateDialogBoxFlag    ="@RootFlag"
                    ResizableDialogBoxFlag ="@ResizableFlag"
                    CompactDialogBoxFlag   ="@CompactFlag"
                    ContentPanelVisibleFlag="@ContentVisibleFlag"
                    TitleBarVisibleFlag    ="@TitleBarVisibleFlag"
                    AnchorPoint            ="@AnchorPoint"
                    ConnectionPoint        ="@OffsetPoint"
                    VisibleFlag            ="@Visibility"
                    InitialDockArea        ="@InitialDockArea"
                    ExtractedFlag          ="@ExtractedFlag"
                    StandardButtonClick    ="@StandardButtonClick"
                    PanelDeleted           ="@ViewDeletedCB"
                    CloseButtonDialogBoxFlag ="@CloseButtonFlag"
                    UserLifeCycleFlag        ="@UserLifeCycleFlag"
                    HandlePanel              ="@HandleEvent"
                    PanelClosed            ="@CloseCB">
      <CATVidLayStack VisibleFlag="@Visibility"
                      syp:name="Container"
                      Orientation="@Orientation"
                      StackAttachmentAppendPolicy="StretchAttachmentPolicy"/>
    </CATAfrSmartCtl>
  </DataTemplate>
  
