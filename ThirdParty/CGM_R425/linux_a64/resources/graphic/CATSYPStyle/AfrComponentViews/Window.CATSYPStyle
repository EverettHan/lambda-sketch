                    CloseButtonDialogBoxFlag   ="@CloseButtonFlag"
                    MinimizeFlag               ="@MinimizeFlag Mode=TwoWays"
                    HandlePanel                ="@HandleEvent"
                    ExtractedFlag              ="@ExtractedFlag">
      <CATVidLayGrid syp:name="MainContainer"
                     GridWeight="row1=1 col1=1">
        <CATVidLayGrid syp:name="ContainerToolbar"
                       CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"
                       GridWeight="col1=1">
          <CATVidCtl syp:name="AccessToolbarLeft"
                     CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=LTB"/>
          <CATVidGPSquare syp:name="AccessToolbarSpace"
                          CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB"/>
