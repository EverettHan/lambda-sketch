        <CATSYPSetter IndexName="AccessToolbarLeft"  Property="IndexedConstraints" TargetName="ContainerToolbar" Value="x=0 y=0 attach=LTB"/>
        <CATSYPSetter IndexName="AccessToolbarSpace" Property="IndexedConstraints" TargetName="ContainerToolbar" Value="x=1 y=0 attach=RLTB"/>
        <CATSYPSetter IndexName="AccessToolbar"      Property="IndexedConstraints" TargetName="ContainerToolbar" Value="x=2 y=0 attach=RTB"/>
