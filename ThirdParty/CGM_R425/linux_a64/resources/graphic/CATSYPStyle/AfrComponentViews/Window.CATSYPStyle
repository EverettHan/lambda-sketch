        <CATSYPSetter IndexName="AccessToolbarSpace" Property="IndexedConstraints" TargetName="ContainerToolbar" Value="x=0 y=1 attach=RLTB"/>
        <CATSYPSetter IndexName="AccessToolbar"      Property="IndexedConstraints" TargetName="ContainerToolbar" Value="x=0 y=2 attach=TB"/>
