        <CATSYPSetter Property="EnableFlag" TargetName="ProgramPlayButton" Value="1"/>
        <CATSYPSetter Property="EnableFlag" TargetName="ProgramStopButton" Value="1"/>
        <CATSYPSetter Property="EnableFlag" TargetName="AnimationPlayReverseButton" Value="1"/>



        <CATSYPSetter Property="EnableFlag" TargetName="UndefPauseButton" Value="1"/>
