  <Template syp:name="InActionBar" Target="CATAfrVidPlayerPresenter">

    <CATVidCtl syp:name="PlayerContainer"/>

  </Template>


  <Template syp:name="SynchronizationPanelContent" Target="CATVidCtl">
    <CATVidLayGrid GridWeight="row1=1" CellSpacing="8">
      <CATVidCtlCheckBox syp:name="CheckBox" syp:Template="Switch" KeyboardManipulationFlag="0" Label="Synchronize selected simulations" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
      <CATVidLayLayer CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB">
        <CATVidGPSquare BorderColor="210 210 210 255"/>
        <CATVidLayGrid GridWeight="row1=1 col1=1">
          <CATVidSpacer RequestedMinimumDimension="1 1" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
          <CATVidSpacer RequestedMinimumDimension="1 1" CATVidLayGrid.IndexedConstraints="x=2 y=2 attach=RLTB"/>
          <CATVidCtlScrollableArea RequestedMinimumDimension="150 200" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB">
            <CATVidLayStack syp:name="PlayersList" CellBorder="5" Orientation="Vertical" StackAttachmentAppendPolicy="StretchAttachmentPolicy" />
          </CATVidCtlScrollableArea>
        </CATVidLayGrid>
      </CATVidLayLayer>

    </CATVidLayGrid>
  </Template>

  <Template Target="CATAfrVidPlayerStyle" syp:name="TimeControlPanelContent">
    <CATVidLayGrid GridWeight="row1=1" CellSpacing="8">
      <CATVidCtlSimpleCombo CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=L" syp:name="TimeControlMode" Name="TimeControlMode"  RequestedMinimumDimension="60 20"/>

      <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RLTB" syp:name="TimeDisplay">
        <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=0 y=0" syp:name="Speed" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" InnerCellSpacing="2">
          <CATVidCtlSlider syp:name="SpeedSlider" Name="SpeedSlider" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=L xSpan=2" RequestedMinimumDimension="170 36" TickPosition="TicksBelow"/>
          <CATVidSpacer RequestedMinimumDimension="10 10" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLTB"/>
          <CATVidCtlSimpleCombo syp:name="SpeedValue" Name="SpeedValue" CATVidLayGrid.IndexedConstraints="x=3 y=0 attach=R" RequestedMinimumDimension="70 20"/>
          <CATVidGPLabel CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=L" RequestedMinimumDimension="46 15" Text="Slow" Name="slowtext" syp:name="slowtext"/>
          <CATVidGPLabel CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=R" RequestedMinimumDimension="46 15" Text="Fast" Name="fasttext" syp:name="fasttext"/>
        </CATVidLayGrid>

        <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=L" syp:name="Duration" VisibleFlag="0" LayoutWhenUnvisibleFlag="0">
          <CATVidGPLabel CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=L" RequestedMinimumDimension="170 15" Name="animationtext" syp:name="animationtext"/>
          <CATVidCtlLineEditor syp:name="playbackLength" Name="playbackLength" CATVidLayGrid.IndexedConstraints="x=1 y=0" RequestedMinimumDimension="60 20" PlaceholderText="hh:mm:ss"/>
        </CATVidLayGrid>

        <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=L" syp:name="FPS" VisibleFlag="0" LayoutWhenUnvisibleFlag="0">
          <CATVidGPLabel CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=L" RequestedMinimumDimension="170 15" Name="fpstext" syp:name="fpstext"/>
          <CATVidCtlDoubleSpinBox syp:name ="fpsValue" Name="fpsValue" CATVidLayGrid.IndexedConstraints="x=1 y=0"  RequestedMinimumDimension="60 20"/>
        </CATVidLayGrid>

        <CATVidLayGrid CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=L" syp:name="Maximum" VisibleFlag="0" LayoutWhenUnvisibleFlag="0" RequestedMinimumDimension="230 10">
        </CATVidLayGrid>

      </CATVidLayGrid>
    </CATVidLayGrid>
  </Template>

