    <AfrActionBarPlayerSectionCtl syp:name="ProtoActionBarPlayerSection" syp:Template="Default" VisibleFlag="@PlayerVisibleFlag ElementName=Presenter" PlayerVisibleFlag="@PlayerVisibleFlag ElementName=Presenter"
                                  LayoutWhenUnvisibleFlag="0"
                                    
                                    >
      <CATAfrVidPlayerPresenter syp:name="Presenter"  LayoutWhenUnvisibleFlag="0" VisibleFlag="@PlayerVisibleFlag ElementName=Presenter" PlayerVisibleFlag="@Visibility Mode=OneWayToSource" syp:Template="InActionBar"></CATAfrVidPlayerPresenter>
    </AfrActionBarPlayerSectionCtl>
  </DataTemplate>
  <!-- DataTemplate to use with ActionBar V3 style to put the Player in its own panel -->
