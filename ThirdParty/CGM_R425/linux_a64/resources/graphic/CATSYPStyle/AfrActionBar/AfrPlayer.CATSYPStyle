<?xml version="1.0"?>

<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <Resources>
    <ResourceDictionary Source="AfrActionBar/AfrActionBarMisc.CATSYPStyle" />
    <ResourceDictionary Source="ActionBarXAfrProto.CATSYPStyle" />
  </Resources>

  <!-- ****************************************************************************************************-->
  <!-- ##########################           Player style templates           ##############################-->
  <!-- ****************************************************************************************************-->


  <!--!!!!   Default Player -->
  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleUndef">
    <!--  Layer -->
    <CATVidLayLayer RequestedMinimumDimension="20 40" >


      <CATVidLayGrid   CellSpacing="2">
        <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=TB">
          <!--<AfrActionBarVidButton Name="BackwardButton"  syp:name="BackwardButton" syp:Template="Default" VisibleFlag="@PlayOn" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBackward.png"  Click="@HandleEventBackward"  />-->
          <AfrActionBarVidButton Name="BackButton"  syp:name="BackButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBack.png"   Click="@HandleEventBack"  />

          <AfrActionBarVidButton Name="PauseButton"  syp:name="PauseButton" syp:Template="Default" VisibleFlag="@PlayOn"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPause.png"   Click="@HandleEventPause" />
          <AfrActionBarVidButton Name="PlayButton"  syp:name="PlayButton" syp:Template="Default" VisibleFlag="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlay.png"   Click="@HandleEventPlay" />

          <AfrActionBarVidButton Name="NextButton"  syp:name="NextButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerNext.png"   Click="@HandleEventNext" />

          <AfrActionBarVidButton Name="SynchroButton"  syp:name="SynchroButton" VisibleFlag="@SynchronizationSupported" CheckState="@SynchronizationActivated" CheckMode="1" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerSynchronization.png"   Click="@HandleEventSynchronization" />

          <!--<AfrActionBarVidButton Name="ForwardButton"  syp:name="ForwardButton" syp:Template="Default" VisibleFlag="@PlayOn" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerForward.png"   Click="@HandleEventForward"  />-->
          <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close.png"   Click="@HandleEventClose" />

        </CATVidLayStack>
        <CATVidLayLayer syp:name="SliderLayer" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" Name="SliderLayer" VisibleFlag="@SliderVisibleFlag">
          <CATVidCtlSlider syp:name="Slider" Name="Slider"
          Orientation="Horizontal" Value="@PercentValue Mode=TwoWays" MinimumValue="0" MaximumValue="100"/>
          <CATVidGPSquare syp:name="NoPicklayer"
            Name="NoPicklayer" LayoutWhenUnvisibleFlag="0" PickFlag="1" UseManipulatorFlag="1"
             VisibleFlag="@SliderInNoPick"/>
        </CATVidLayLayer>

      </CATVidLayGrid>
    </CATVidLayLayer>

  </Template>

  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleUndef" syp:version="1">
    <!--  Layer -->
    <CATVidLayLayer RequestedMinimumDimension="20 40" >

      <CATVidLayGrid  GridWeight="col1=1" CellSpacing="2">
        <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=TB">
          <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close_v1.png"   Click="@HandleEventClose" />
          <CATVidGPSquare Texture="ActionBar/ABDivider.png" RequestedMinimumDimension="4 45" />
          <AfrActionBarVidButton Name="BackButton"  syp:name="BackButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBack_v1.png"   Click="@HandleEventBack"  />
          <AfrActionBarVidButton Name="PauseButton"  syp:name="PauseButton" syp:Template="Default" VisibleFlag="@PlayOn"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPause_v1.png"   Click="@HandleEventPause" />
          <AfrActionBarVidButton Name="PlayButton"  syp:name="PlayButton" syp:Template="Default" VisibleFlag="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlay_v1.png"   Click="@HandleEventPlay" />
          <AfrActionBarVidButton Name="NextButton"  syp:name="NextButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerNext_v1.png"   Click="@HandleEventNext" />
          <AfrActionBarVidButton Name="SynchroButton"  syp:name="SynchroButton" VisibleFlag="@SynchronizationSupported" CheckState="@SynchronizationActivated" CheckMode="1" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerSynchronization_v1.png"   Click="@HandleEventSynchronization" />

        </CATVidLayStack>
        <CATVidLayLayer syp:name="SliderLayer" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB" Name="SliderLayer" VisibleFlag="@SliderVisibleFlag">
          <CATVidCtlSlider syp:name="Slider" Name="Slider"
          Orientation="Horizontal" Value="@PercentValue Mode=TwoWays" MinimumValue="0" MaximumValue="100" RequestedMinimumDimension="100 50"/>
          <CATVidGPSquare syp:name="NoPicklayer"
            Name="NoPicklayer" LayoutWhenUnvisibleFlag="0" PickFlag="1" UseManipulatorFlag="1"
             VisibleFlag="@SliderInNoPick"/>
        </CATVidLayLayer>
      </CATVidLayGrid>
    </CATVidLayLayer>
  </Template>
  
  <!--Large Default Player -->
  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleUndef" syp:Variation="{490,40}">
    <!--  Layer -->



    <CATVidLayGrid  RequestedMinimumDimension="20 40" GridWeight="col1=1" CellSpacing="2">
      <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLT">

        <AfrActionBarVidButton Name="BackButton"  syp:name="BackButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBack.png"   Click="@HandleEventBack"  />
        <AfrActionBarVidButton Name="PauseButton"  syp:name="PauseButton" syp:Template="Default" VisibleFlag="@PlayOn"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPause.png"   Click="@HandleEventPause" />
        <AfrActionBarVidButton Name="PlayButton" Tooltip="Should not see this text" syp:name="PlayButton" syp:Template="Default" VisibleFlag="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlay.png"   Click="@HandleEventPlay" />
        <AfrActionBarVidButton Name="NextButton"  syp:name="NextButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerNext.png"   Click="@HandleEventNext" />
        <AfrActionBarVidButton Name="SynchroButton"  syp:name="SynchroButton" VisibleFlag="@SynchronizationSupported" CheckState="@SynchronizationActivated" CheckMode="1" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerSynchronization.png"   Click="@HandleEventSynchronization" />


      </CATVidLayStack>


      <CATVidLayGrid GridWeight="col1=1 row1=1" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLT" >
        <CATVidSpacer RequestedMinimumDimension="2 8" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
        <CATVidLayLayer syp:name="SliderLayer" CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB" Name="SliderLayer" VisibleFlag="@SliderVisibleFlag">
          <CATVidCtlSlider syp:name="Slider" Name="Slider"
          Orientation="Horizontal" Value="@PercentValue Mode=TwoWays" MinimumValue="0" MaximumValue="100"/>
          <CATVidGPSquare syp:name="NoPicklayer"
            Name="NoPicklayer" LayoutWhenUnvisibleFlag="0" PickFlag="1" UseManipulatorFlag="1"
             VisibleFlag="@SliderInNoPick"/>
        </CATVidLayLayer>
        <CATVidSpacer RequestedMinimumDimension="2 2" CATVidLayGrid.IndexedConstraints="x=2 y=2 attach=RLTB"/>
      </CATVidLayGrid>

      <CATVidLayStack  Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=2 y=0 attach=RLT">
        <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close.png"   Click="@HandleEventClose" />
      </CATVidLayStack>
    </CATVidLayGrid>
  </Template>

  <!--Large Default Player -->
  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleUndef" syp:Variation="{490,40}" syp:version="1">
    <!--  Layer -->
    <CATVidLayGrid  RequestedMinimumDimension="20 40" GridWeight="col1=1" CellSpacing="2">
      <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLT">

        <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close_v1.png"   Click="@HandleEventClose" />
        <CATVidGPSquare Texture="ActionBar/ABDivider.png" RequestedMinimumDimension="4 45" />
        <AfrActionBarVidButton Name="BackButton"  syp:name="BackButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBack_v1.png"   Click="@HandleEventBack"  />
        <AfrActionBarVidButton Name="PauseButton"  syp:name="PauseButton" syp:Template="Default" VisibleFlag="@PlayOn"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPause_v1.png"   Click="@HandleEventPause" />
        <AfrActionBarVidButton Name="PlayButton"  syp:name="PlayButton" syp:Template="Default" VisibleFlag="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlay_v1.png"   Click="@HandleEventPlay" />
        <AfrActionBarVidButton Name="NextButton"  syp:name="NextButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerNext_v1.png"   Click="@HandleEventNext" />
        <AfrActionBarVidButton Name="SynchroButton"  syp:name="SynchroButton" VisibleFlag="@SynchronizationSupported" CheckState="@SynchronizationActivated" CheckMode="1" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerSynchronization_v1.png"   Click="@HandleEventSynchronization" />

      </CATVidLayStack>

      <CATVidLayLayer syp:name="SliderLayer" CATVidLayGrid.IndexedConstraints="x=1 y=0 attach=RLTB" Name="SliderLayer" VisibleFlag="@SliderVisibleFlag">
