  <!--!!!!   Animation Player -->
  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleAnimation">
    <!--  Layer -->

    <CATVidLayGrid RequestedMinimumDimension="20 80" GridWeight="col0=1 row1=1"  CellSpacing="2" >


      <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=TB">

        <CATVidLayLayer RequestedMinimumDimension="40 40">
          <AfrActionBarVidButton Name="BackwardButton"  syp:name="BackwardButton" syp:Template="Default" VisibleFlag="@PlayOn" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBackward.png"  Click="@HandleEventBackward"   />
          <AfrActionBarVidButton Name="BackButton"  syp:name="BackButton" syp:Template="Default" VisibleFlag="@PlayOff" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerBack.png"   Click="@HandleEventBack"  />
        </CATVidLayLayer>
        <AfrActionBarVidButton Name="PlayReverseButton"   syp:name="PlayReverseButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlayReverse.png"  Click="@HandleEventPlayReverse" />
        <AfrActionBarVidButton Name="PauseButton"  syp:name="PauseButton" syp:Template="Default" CheckMode="1" CheckState="@PlayOff"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPause.png"   Click="@HandleEventPause" />
        <AfrActionBarVidButton Name="PlayButton"  syp:name="PlayButton" syp:Template="Default" CheckMode="1" CheckState="@PlayOn"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerPlay.png"   Click="@HandleEventPlay" />
        <CATVidLayLayer RequestedMinimumDimension="40 40">
          <AfrActionBarVidButton Name="NextButton"  syp:name="NextButton" syp:Template="Default" VisibleFlag="@PlayOff" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerNext.png"   Click="@HandleEventNext" />
          <AfrActionBarVidButton Name="ForwardButton"  syp:name="ForwardButton" syp:Template="Default" VisibleFlag="@PlayOn" LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerForward.png"   Click="@HandleEventForward"  />
        </CATVidLayLayer>
        <CATVidLayLayer >
          <AfrActionBarVidButton Name="LoopRunOnceButton" syp:name="LoopRunOnceButton" syp:Template="Default"
                                LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                               Icon="Player/I_CompassPlayerLoopRunOnce.png"
                               RequestedMinimumDimension="40 40" IconDimension="32 32"  Click="@HandleEventLoop" VisibleFlag="1"/>
          <AfrActionBarVidButton Name="LoopRunContinuouslyButton" syp:name="LoopRunContinuouslyButton" syp:Template="Default"
                                LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                               Icon="Player/I_CompassPlayerLoopRunContinuously.png"
                               RequestedMinimumDimension="40 40" IconDimension="32 32"  Click="@HandleEventLoop" VisibleFlag="0"/>
          <AfrActionBarVidButton Name="LoopRunContinuouslyBackAndForthButton" syp:name="LoopRunContinuouslyBackAndForthButton" syp:Template="Default"
                                LayoutWhenUnvisibleFlag="0" FocusableFlag="0"
                               Icon="Player/I_CompassPlayerLoopRunContinuouslyBackAndForth.png"
                               RequestedMinimumDimension="40 40" IconDimension="32 32"  Click="@HandleEventLoop" VisibleFlag="0"/>
        </CATVidLayLayer>

        <CATVidLayLayer syp:name="HamburgerMenu" LayoutWhenUnvisibleFlag="0">
          <AfrActionBarVidButton Name="HamburgerMenuButton"  syp:name="HamburgerMenuButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerHamburger.png" EnableFlag="1" ShowClipped="0"
                                      CheckMode="1" ForegroundColor="0 0 0 255" OverFlagIndirect="@CheckState ElementName=HamburgerMenuButton"  TextHeight="14" FontSize="11" />
          <CATVidCtlPopup syp:name="HamburgerMenuPopup" LayoutWhenUnvisibleFlag="0" ConsumeMouseEventFlag="0" SnapFlag="1" SnapSide="Top" SnapAlignment="SnapAlignRight" PopupCompactFlag="1" VisibleFlag="@CheckState ElementName=HamburgerMenuButton Mode=TwoWays" ModalFlag="1" AutoHideFlag="1">
            <CATVidLayLayer>
              <CATVidGPSquare syp:name="HamburgerMenuPopupBkgnd" UseManipulatorFlag="1" Color="234 234 234 220"/>
              <CATVidLayGrid>
                <CATVidLayGrid GridWeight="row1=1 col1=1 " RequestedMinimumDimension="60 20" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB">
                  <CATVidSpacer RequestedMinimumDimension="2 2" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB"/>
                  <CATVidLayLayer  CATVidLayGrid.IndexedConstraints="x=1 y=1 attach=RLTB">
                    <CATVidCtlLineEditor syp:name="Editor" Alignment="AlignRight"  EditingFinished="@HandleEventEditorChanged" ReturnPressed="@HandleEventEditorChanged"  Name="Editor" Text="@StringValue Mode=TwoWays"  ReadOnlyFlag="0" />
                    <CATVidGPLabel CropPolicy="NoCrop" VisibleFlag="0" Text="@StringValue"/>
                    <!--By pass to give to editor the same width of the text-->
                  </CATVidLayLayer>

                  <CATVidSpacer RequestedMinimumDimension="2 2" CATVidLayGrid.IndexedConstraints="x=2 y=2 attach=RLTB"/>
                </CATVidLayGrid>
                <CATVidLayStack  CATVidLayGrid.IndexedConstraints="x=0 y=1 attach=RTB">
                  <AfrActionBarVidButton Name="FirstButton"  syp:name="FirstButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerFirst.png"   Click="@HandleEventFirst" />
                  <AfrActionBarVidButton Name="LastButton"  syp:name="LastButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerLast.png"   Click="@HandleEventLast" />
                  <AfrActionBarVidButton Name="OptionsButton"  syp:name="OptionsButton" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerOptions.png"   Click="@HandleEventOptions" />
                  <AfrActionBarVidButton Name="SynchroButton"  syp:name="SynchroButton" VisibleFlag="@SynchronizationSupported" CheckState="@SynchronizationActivated" CheckMode="1" syp:Template="Default"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_CompassPlayerSynchronization.png"   Click="@HandleEventSynchronization" />

                </CATVidLayStack>
              </CATVidLayGrid>
            </CATVidLayLayer>
          </CATVidCtlPopup>
        </CATVidLayLayer>
        <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default" LongTooltipStyleSheet="CATAfrBasicStyle.CATSYPStyle"
                            LongTooltipTemplateName="DefaultAfrLongTooltip"
                            TooltipTitle="Exit"  Tooltip="Exit !!!!!"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close.png"   Click="@HandleEventClose" />
      </CATVidLayStack>
      <CATVidLayLayer syp:name="SliderLayer" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=RLTB" Name="SliderLayer" VisibleFlag="@SliderVisibleFlag">
        <CATVidCtlSlider syp:name="Slider" Name="Slider"
        Orientation="Horizontal" Value="@PercentValue Mode=TwoWays" MinimumValue="0" MaximumValue="100"/>
        <CATVidGPSquare syp:name="NoPicklayer"
          Name="NoPicklayer" LayoutWhenUnvisibleFlag="0" PickFlag="1" UseManipulatorFlag="1"
           VisibleFlag="@SliderInNoPick"/>
      </CATVidLayLayer>





    </CATVidLayGrid>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunOnce">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>
      </CATSYPTrigger>

      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunContinuously">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="1"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="0"/>

      </CATSYPTrigger>

      <CATSYPTrigger Property="Loop" Value="CATPLMCompassMaxiPlayerLoopRunContinuouslyBackAndForth">
        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="1"/>

        <CATSYPSetter TargetName="LoopRunOnceButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyButton" Property="VisibleFlag" Value="0"/>
        <CATSYPSetter TargetName="LoopRunContinuouslyBackAndForthButton" Property="VisibleFlag" Value="1"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="FocusedFlag" TargetName="Editor" Value="1">
        <CATSYPSetter Property="BorderVisibleFlag" TargetName="Editor" Value="1"/>
        <CATSYPSetter Property="BackgroundColor" TargetName="Editor" Value="EditBackground 255"/>
      </CATSYPTrigger>
      <CATSYPTrigger Property="FocusedFlag" TargetName="Editor" Value="0">
        <CATSYPSetter Property="BorderVisibleFlag" TargetName="Editor" Value="0"/>
        <CATSYPSetter Property="BackgroundColor" TargetName="Editor" Value="0 0"/>
      </CATSYPTrigger>


    </CATSYPTriggerCollection>
  </Template>
  
  <!--!!!!   Animation Player -->
  <Template Target="CATAfrVidPlayerStyle" syp:name="CATPLMCompassMaxiPlayerStyleAnimation" syp:version="1">
    <!--  Layer -->

    <CATVidLayGrid RequestedMinimumDimension="20 50" GridWeight="col1=1"  CellSpacing="2" >


      <CATVidLayStack syp:name="Stack" Orientation="Horizontal" CATVidLayGrid.IndexedConstraints="x=0 y=0 attach=TB">

        <AfrActionBarVidButton Name="ExitButton"  syp:name="ExitButton" syp:Template="Default" LongTooltipStyleSheet="CATAfrBasicStyle.CATSYPStyle"
                            LongTooltipTemplateName="DefaultAfrLongTooltip"
                            TooltipTitle="Exit"  Tooltip="Exit !!!!!"  LayoutWhenUnvisibleFlag="0" FocusableFlag="0" RequestedMinimumDimension="40 40" IconDimension="32 32" Icon="Player/I_Close_v1.png"   Click="@HandleEventClose" />
        <CATVidGPSquare Texture="ActionBar/ABDivider.png" RequestedMinimumDimension="4 45" />
