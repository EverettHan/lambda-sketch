  <!--New read-only view with font-icons for boolean tweaker-->
  <Template syp:name="FormLineReadOnly_bool" Target="CATVidCtlTweakerint">
    <CATVidLayGrid GridWeight="col1=1">
      <CATVidGPFontIcon FontSize="16" RequestedMinimumDimension="0 21" Name="Value" syp:name="_AutoColoredText"/>
    </CATVidLayGrid>
    <CATSYPTriggerCollection>
      <CATSYPTrigger Property="Value" Value="1">
