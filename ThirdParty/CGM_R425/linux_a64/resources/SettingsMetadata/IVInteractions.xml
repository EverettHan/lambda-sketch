<?xml version="1.0" encoding="utf-8"?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd"
               xmlns:st="urn:com:dassault_systemes:settings"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <st:Description name="IVInteractions" PrimaryTabpage="/General/VirtualReality">
    <st:role>Setting to handle the immersive interactions parameters</st:role>
    <st:detailedRole>Setting to handle the immersive interactions parameters</st:detailedRole>
    <st:RepositoryFile>IVInteractions</st:RepositoryFile>
    <st:version>2</st:version>
    <st:compatibility>
      <st:FullCompatibilityfromLevel>2</st:FullCompatibilityfromLevel>
      <st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
    </st:compatibility>
  </st:Description>
  <st:attributes>
    <st:attribute Name="CursorLook">
      <st:role>Determines the look of the immersive 3D cursor</st:role>
      <st:boolean StorageType="uint">
        <st:default>
          <st:value>false</st:value>
        </st:default>
      </st:boolean>
    </st:attribute>
    <st:attribute Name="MultiTouch">
      <st:role>Determines if multi-touch based interactions are allowed or not</st:role>
      <st:boolean StorageType="uint">
        <st:default>
          <st:value>false</st:value>
        </st:default>
      </st:boolean>
    </st:attribute>
  </st:attributes>
</st:repository>
