<?xml version="1.0" encoding="utf-8"?>
<st:repository xsi:schemaLocation="urn:com:dassault_systemes:settings settings.xsd"
               xmlns:st="urn:com:dassault_systemes:settings"
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <st:Description name="IVSensorialUser" PrimaryTabpage="/General/VirtualReality">
    <st:role>Setting to handle the sensorial user in interactions parameters</st:role>
    <st:detailedRole>Setting to handle the sensorial user in interactions parameters</st:detailedRole>
    <st:RepositoryFile>IVSensorialUser</st:RepositoryFile>
    <st:version>1</st:version>
    <st:compatibility>
      <st:FullCompatibilityfromLevel>1</st:FullCompatibilityfromLevel>
      <st:UpwardCompatibilityfromLevel>1</st:UpwardCompatibilityfromLevel>
    </st:compatibility>
  </st:Description>
  <st:attributes>
    <st:attribute Name="DominantHand">
      <st:role>Determines the end user dominant hand (left or right) for interactions purposes</st:role>
      <st:int>
        <st:enum>
          <st:role>Left</st:role>
          <st:value>0</st:value>
          <st:role>Right</st:role>
          <st:value>1</st:value>
        </st:enum>
        <st:default>
          <st:value>1</st:value>
        </st:default>
      </st:int>
    </st:attribute>
  </st:attributes>
</st:repository>
