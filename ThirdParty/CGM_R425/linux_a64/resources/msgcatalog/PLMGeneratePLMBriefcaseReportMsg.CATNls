PR_HELP_SUMMARY           = "-summary <TextFileName>         where <TextFileName> is the summary text report output file. (Optional)\n" ;
PR_HELP_FILTER_BY         = "-filterBy <Type> <Value>        where <Type> defines filtering option to be used and value represents the parameter of filtering operation.\n" ;
PR_HELP_EXTENSION         = "-extension                      to display extension information. (Optional)\n"                            ;
PR_HELP_DS_ATTRIBUTES     = "-cd                             to display all Core Data attributes, not only basic ones. (Optional)\n"    ;
PR_HELP_CUSTO_ATTRIBUTES  = "-ee                             to display all exposed Extended Enterprise attributes. (Optional)\n"       ;
PR_HELP_OTHER_OID         = "-oid                            to display Major Id & Logical Id as attributes. (Optional)\n"              ;
