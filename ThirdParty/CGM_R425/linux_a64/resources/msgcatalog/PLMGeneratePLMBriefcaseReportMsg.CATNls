PR_HELP_GENERAL_REPORT    = "Usage: PLMGeneratePLMBriefcaseReport  <flags as described below>\n" ;
PR_HELP_OUTPUT            = "-output <ReportFileName>        where <ReportFileName> is the XML report output file. (Mandatory)\n"       ;
