<?xml version="1.0"?>
<Styles xmlns:syp="http://www.3ds.com/xsd/SYP">

  <!--======================================-->
  <!-- Home Page Url viewer Component CID-->
  <!--======================================-->
  <Template syp:name="CAfrHomePageUrlViewer" Target="CATAfrComponent">
    <CATCAfrWindow Name               = "CAfrHomePageUrlViewer"
                   Title              = "IF WE NAVIGATOR"
                   ResizableFlag      = "1"
                   ContentVisibleFlag ="1"
                   CompactFlag        ="0"
                   RepresentationType = "AfrVIDFraPanel">
