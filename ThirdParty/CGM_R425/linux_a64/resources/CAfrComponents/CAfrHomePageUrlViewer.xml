  <Template syp:name="CAfrHomePageUrlViewerDialog" Target="CATAfrComponent">
    <CATCAfrUrlViewer Name       ="HomePageUrlViewer"
                      Visibility ="1"
                      Silent     = "1"
                      Url="http://dell621dsy/Coachmark/win_b64/webroot/plugins/Coachmark/Coachmark.html"
                      RepresentationType="CATCAfrUrlViewer_Dialog"
                      Dimension  = "624 484">
    </CATCAfrUrlViewer>
  </Template>
  
  <!--======================================-->
  <!-- Home Page Url viewer Component CID-->
  <!--======================================-->
  <Template syp:name="CAfrHomePageUrlViewerWeb" Target="CATAfrComponent">
    <CATCAfrWindow Name               = "CAfrHomePageUrlViewerDialog"
                   Visibility         ="1"
                   RepresentationType = "AfrDialogBox">
      <CATCAfrForm Name="Form_HomePageUrlViewer"
                   Visibility ="1">
