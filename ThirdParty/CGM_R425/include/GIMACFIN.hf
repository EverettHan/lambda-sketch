C
C  MACRO CATGEO POUR GESTION INTERNE DES ERREURS ET EMPILAGE CATGEO
C  DERNIERE CARTE EXECUTABLE DES ROUTINES CATGEO AVANT RETURN 1
C
C----------------------------------------------------------------------
C
C A- MEMORISATION DU TYPE DE SORTIE A EFFECTUER
C
        LGIUFI = .NOT. GIUFIN( IER )
C
C B- GESTION DE L'EMPILAGE DES ERREURS
C
       CALL GIUCLO( 1 )
C
C C- GESTION DE LA SORTIE SUR LABEL
C
       IF( LGIUFI ) RETURN0
C
