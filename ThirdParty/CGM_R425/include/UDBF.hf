C  CONTENU :  DESCRIPTION DE L'UDB
C             LES FONCTIONS LOGIQUES
C
C**********************************************************************
C
      T_UDB(UDB_TYPE,PRV,ESPECE) =
     *    (UDB_TYPE.GE.1.AND.UDB_TYPE.LE.ITIDES_MAX(PRV,ESPECE))
C
      T_NUMDES(UDB_TYPE,PRV,ESPECE)=(UDB_TYPE+NUMDES_DELTA(PRV,ESPECE))
C
      N_PRIVE(NUMDES)=(
     *      (NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_CATIA).AND.NUMDES.LE.(
     * NUMDES_DELTA(P_PRIVE,UDB_CATIA)+ITIDES_MAX(P_PRIVE,UDB_CATIA)))
     *  .OR.(NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_MAO).AND.NUMDES.LE.(
     * NUMDES_DELTA(P_PRIVE,UDB_MAO)+ITIDES_MAX(P_PRIVE,UDB_MAO)))
     *  .OR.(NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_CKM).AND.NUMDES.LE.(
     * NUMDES_DELTA(P_PRIVE,UDB_CKM)+ITIDES_MAX(P_PRIVE,UDB_CKM))) )
C
      N_CATIA(NUMDES)=(
     *      (NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_CATIA).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PRIVE,UDB_CATIA)+ITIDES_MAX(P_PRIVE,UDB_CATIA)))
     *  .OR.(NUMDES.GT.NUMDES_DELTA(P_PUBLIC,UDB_CATIA).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PUBLIC,UDB_CATIA)+ITIDES_MAX(P_PUBLIC,UDB_CATIA))))
C
      N_MAO(NUMDES)=(
     *      (NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_MAO).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PRIVE,UDB_MAO)+ITIDES_MAX(P_PRIVE,UDB_MAO)))
     *  .OR.(NUMDES.GT.NUMDES_DELTA(P_PUBLIC,UDB_MAO).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PUBLIC,UDB_MAO)+ITIDES_MAX(P_PUBLIC,UDB_MAO))))
C
      N_CKM(NUMDES)=(
     *      (NUMDES.GT.NUMDES_DELTA(P_PRIVE,UDB_CKM).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PRIVE,UDB_CKM)+ITIDES_MAX(P_PRIVE,UDB_CKM)))
     *  .OR.(NUMDES.GT.NUMDES_DELTA(P_PUBLIC,UDB_CKM).AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PUBLIC,UDB_CKM)+ITIDES_MAX(P_PUBLIC,UDB_CKM))))
C
      N_DUPLI(NUMDES)=(
     *      (NUMDES .GT. (NUMDES_DELTA(P_PRIVE,UDB_MAO)
     *                    + ITIDES_MAX(P_PRIVE,UDB_MAO)/2)
     *  .AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PRIVE,UDB_MAO)+ITIDES_MAX(P_PRIVE,UDB_MAO)))
     *  .OR.(NUMDES .GT. (NUMDES_DELTA(P_PUBLIC,UDB_MAO)
     *                    + ITIDES_MAX(P_PUBLIC,UDB_MAO)/2)
     *  .AND.NUMDES.LE.(
     *NUMDES_DELTA(P_PUBLIC,UDB_MAO)+ITIDES_MAX(P_PUBLIC,UDB_MAO))))
C
      N_UDB  (NUMDES) = ( N_CATIA(NUMDES) .OR. N_MAO(NUMDES)
     *                    .OR. N_CKM(NUMDES) )
C
      N_ITIDES(NUMDES,PRV,ESPECE) = (NUMDES-NUMDES_DELTA(PRV,ESPECE))
C
