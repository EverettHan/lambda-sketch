//=======================================================================
//function : Empty constructor
//purpose  : 
//=======================================================================

inline CATTCollection_Sequence::CATTCollection_Sequence()
{
}

//=======================================================================
//function : Append 
//purpose  : Appends a Sequence to myself
//=======================================================================

inline void  CATTCollection_Sequence::Append (CATTCollection_Sequence& S)
{
  if (FirstItem == S.FirstItem) Assign(S);
  PAppend (S);
} 
//=======================================================================
//function : Prepend 
//purpose  : Prepends a Sequence to myself
//=======================================================================
inline void CATTCollection_Sequence::Prepend (CATTCollection_Sequence& S)
{
  if (FirstItem == S.FirstItem) Assign(S);
  PPrepend (S);
}
//=======================================================================
//function : InsertBefore
//purpose  : Inserts an item before an index
//=======================================================================
inline void CATTCollection_Sequence::InsertBefore (const CATStandard_Integer Index,
						const SeqItem& I)
{
  InsertAfter(Index-1,I);
}

//=======================================================================
//function : InsertBefore
//purpose  : Inserts a Sequence before an index
//=======================================================================
inline void CATTCollection_Sequence::InsertBefore (const CATStandard_Integer Index,
					         CATTCollection_Sequence& S)
{
  InsertAfter(Index-1,S);
}

//=======================================================================
//function : InsertAfter
//purpose  : Inserts a Sequence after an index
//=======================================================================
inline void CATTCollection_Sequence::InsertAfter (const CATStandard_Integer Index,
					       CATTCollection_Sequence& S)
{
  PInsertAfter(Index,S);
}
//=======================================================================
//function : Split
//purpose  : Cuts a Sequence into two
//=======================================================================
inline void CATTCollection_Sequence::Split (const CATStandard_Integer Index,
					  CATTCollection_Sequence& S)
{
  S.Clear();
  PSplit(Index,S);
}






