C----------------------------------------------------------------------
C
C  FONCTIONS DE CONVERSION A PARTIR
C
C    TYPE_LOGIGUE X MODE_PROTECTION X UDB_ESPECE
C      --->  L    T_UDB      C'EST UN BON TYPE TYPE LOGIQUE
C      --->  I*4  T_NUMDES   NUMERO DE DESCRIPTION CATIA
C
C  FONCTIONS DE CONVERSION A PARTIR :   NUMERO DE DESCRIPTION CATIA
C      --->  L    N_UDB      C'EST UNE DESCRIPTION D'UDB
C      --->  L    N_PRIVE    C'EST UN UDB DE MODE DE PROTECTION PRIVE
C      --->  L    N_CATIA    UDB D'ESPECE CATIA
C      --->  L    N_MAO      UDB D'ESPECE MAO ( UDB V2 )
C      --->  L    N_CKM      UDB D'ESPECE CKM
C      --->  L    N_DUPLI    UDB D'ESPECE DUPLICABLE
C      --->  I*4  N_ITIDES   TYPE LOGIQUE CATIA
C
C----------------------------------------------------------------------
C
C---- LES PARAMETRES DES FONCTIONS LOGIQUES
C
         INTEGER*4  NUMDES
         INTEGER*4  UDB_TYPE
         INTEGER*4  PRV
         INTEGER*4  ESPECE
C
C---- LES FONCTIONS LOGIQUES
C
         LOGICAL    T_UDB
         INTEGER*4  T_NUMDES
C
         LOGICAL    N_UDB
         LOGICAL    N_PRIVE
         LOGICAL    N_CATIA
         LOGICAL    N_MAO
         LOGICAL    N_CKM
         LOGICAL    N_DUPLI
         INTEGER*4  N_ITIDES
C
C---- LES TABLEAUX DE PARAMETRAGE
C
         INTEGER*4  NUMDES_DELTA
         DIMENSION  NUMDES_DELTA( P_PRIVE:P_PUBLIC ,
     *                            UDB_CATIA:UDB_CKM )
C
         INTEGER*4  ITIDES_MAX
         DIMENSION  ITIDES_MAX( P_PRIVE:P_PUBLIC ,
     *                          UDB_CATIA:UDB_CKM )
C
         DATA NUMDES_DELTA(P_PRIVE, UDB_CATIA) /     8383 /
         DATA ITIDES_MAX(P_PRIVE, UDB_CATIA) /       2000 /
C
         DATA NUMDES_DELTA(P_PRIVE, UDB_MAO    ) /  10383 /
         DATA ITIDES_MAX(P_PRIVE,UDB_MAO)    /       2000 /
C
         DATA NUMDES_DELTA(P_PUBLIC,UDB_MAO    ) /  12383 /
         DATA ITIDES_MAX(P_PUBLIC,UDB_MAO)   /       2000 /
C
         DATA NUMDES_DELTA(P_PRIVE, UDB_CKM    ) /  14383 /
         DATA ITIDES_MAX(P_PRIVE,UDB_CKM)    /        128 /
C
         DATA NUMDES_DELTA(P_PUBLIC,UDB_CKM    ) /  14511 /
         DATA ITIDES_MAX(P_PUBLIC,UDB_CKM)   /        128 /
C
         DATA NUMDES_DELTA(P_PUBLIC,UDB_CATIA) /    16383 /
         DATA ITIDES_MAX(P_PUBLIC,UDB_CATIA) /      16000 /
C
