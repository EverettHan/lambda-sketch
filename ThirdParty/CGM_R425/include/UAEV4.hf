C APPLICATION DATA
C
      COMMON / UAEV4 / UAEOPERA , MAXOPERA , LISTICP , IRNGICP ,IVALPRO
     & , NBOPERA , ICPDECAL , IMINICP ,MSK_PRO , ICHANGE , CHAPPRO
C
C STRUCTURE LOGIQUE DU COMMUN
C
C *** LISTE DES OPERATEURS
C
      CHARACTER*8 UAEOPERA(8)
C
C *** NOMBRE MAXI D'OPERATEURS
C
      INTEGER*4 MAXOPERA
C
C *** NOMBRE EFFECTIF D'OPERATEURS
C
      INTEGER*4 NBOPERA
C
C *** VALEUR DE DECALAGE POUR LA CORRESPONDANCE (IC --> RANG(OPERATEUR))
C
      INTEGER*4 ICPDECAL
C
C *** VALEUR MINIMALE DES IC
C
      INTEGER*4 IMINICP
C
C *** LISTE DES IC CORRESPONDANTS AUX OPERATEURS
C
      INTEGER*4 LISTICP (8)
C
C *** CORRESPONDANCE  IC --> RANG (OPERATEUR)
C *** RANG (OPERATEUR) = IRNGICP ((IC - ICPDECAL) / 2)
C
      INTEGER*4 IRNGICP (8)
C
C *** VALEURS DE PROTECTION DES OPERATEURS
C
      INTEGER*4 IVALPRO (8)
C
C *** MARQUAGE DE LA ZONE PROTEGEE (LZTPR) AVEC BIT D'EFFACEMENT
C ***  (IC-ICPDECAL)/2 --> MARQUAGE
C
      INTEGER*4 MSK_PRO (8)
C
C *** TABLEAU CONTENANT LA CORRESPONDANCE DES IC SI ON VEUT LES
C *** FAIRE INTERPRETER DIFFEREMMENT DANS PROTEG
C
      INTEGER*4 ICHANGE (8)
C
C *** CHAINE D'APPLICATION LIE AU TABLEAU PRECEDENT
C
      CHARACTER*8 CHAPPRO
