   * This example retrieves in <tt>Service1</tt> the <tt>IDService</tt>
   * application's service.
   * <pre>
   * Dim Service1 As Service
   * Set Service1 = CATIA.<font color="red">GetSessionService</font>("IDService")
   * </pre>
   */
	HRESULT GetSessionService(in CATBSTR iService,  out /*IDLRETVAL*/ CATIAService oService);
