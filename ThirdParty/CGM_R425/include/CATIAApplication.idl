	/**
	 * Displays a modal dialog box.
	 * <br><b>Role:</b> This dialog box can be used to select or enter
	 * the name of a file to open or save.
 	 * @param iTitle
	 * The title of the dialog box.
	 * @param iExtension
	 * A file extension filter.
	 * @param iMode
	 * The mode in which to run the dialog box (either <tt>CatFileSelectionModeOpen</tt> or
	 * <tt>CatFileSelectionModeSave</tt>.
	 * @return
	 * A string containing the full path of the selected file, or a zero-length
	 * string if the user selects <tt>Cancel</tt>.
	 * @sample
     * This example asks the user to select a text file and prints the path of the selected
	 * file.
     * <pre>
     * filepath = CATIA.<font color="red">FileSelectionBox("Select a text file", "*.txt", CatFileSelectionModeOpen)</font>
     * CATIA.SystemServices.Print "The selected file is " & filepath
     * </pre>
 	 */
	HRESULT FileSelectionBox(
		in CATBSTR iTitle,
		in CATBSTR iExtension,
		in CatFileSelectionMode iMode,
		inout /*IDLRETVAL*/ CATBSTR oFilePath);

     /**
      * Displays a modal dialog box.
      * <br><b>Role:</b> This dialog box can be used to select or enter
      * the name of a folder.
      * @param iTitle
      * The title of the dialog box.
      * @return
      * A string containing the full path of the selected folder, or a zero-length
      * string if the user selects <tt>Cancel</tt>.
      * @sample
     * This example asks the user to select a folder and prints the path of the selected
      * folder.
     * <pre>
     * folderpath = CATIA.<font color="red">FolderSelectionBox("Select a folder")</font>
     * CATIA.SystemServices.Print "The selected folder is " & folderpath
     * </pre>
      */
     HRESULT FolderSelectionBox(
          in CATBSTR iTitle,
          inout /*IDLRETVAL*/ CATBSTR oFolderPath);

	/**
	 * Starts a command.
	 * <br><b>Role:</b> This method Starts a command and executes it until its first interaction.
     * Please notice that interactions such as selections you could add after in your macro will not work.
     * StartCommand is useful to execute one-shot (not interactive) commands.
     * It is not safe for interactive commands.
	 * @param iCommandId
	 * The identifier of the command to be started. This identifier can be the name of the 
	 * command or its alias. Use the name of the command in the corresponding language installed 
	 * on your machine.
 	 */
	HRESULT StartCommand(
		in CATBSTR iCommandId);

